<script type="text/javascript">

    LayoutApp.controller('MyController', function ($scope, $http, $timeout) {
        $(document).ready(function () {
            $('.my-select').select2({
                placeholder: "--Select--", allowClear: true, width: '100%'
            });
        });

        $(document).ready(function () {

            var queryString = new Array();
            if (window.location.search.split('?').length > 1) {
                var params = window.location.search.split('?')[1].split('&');
                for (var i = 0; i < params.length; i++) {
                    var key = params[i].split('=')[0];
                    var value = decodeURIComponent(params[i].split('=')[1]);
                    queryString[key] = value;
                }
            }
            if (queryString["BLID"] != null) {

                $scope.RegId = queryString["BLID"].toString();
                $scope.RecNumberRecords(queryString["BLID"].toString());
                $scope.HdAgencyID = queryString["AgencyID"].toString();
                $scope.HdVesVoyID = queryString["VesVoyID"].toString();
            }
        });

      

        $scope.RecNumberRecords = null;
        $scope.RecNumberRecords = function () {
            var BussData = {};

            BussData.BLID = $scope.RegId;
            
            $http({

                method: 'POST',
                url: '/api/BLQueryApi/BLRecepNoList/',
                data: JSON.stringify(BussData),
            }).then(function (result) {

                //if (result.data.length > 0) {
                //    $scope.lblPartyName = result.data[0].PartyName;
                //    $scope.lblRecpDate = result.data[0].RecpDate;
                //    $scope.lblRecpTotal = result.data[0].LocalAmount;
                //}
                $scope.RecNumberDetails = result.data;
                $scope.btnPDF($scope.RecNumberDetails[0].RecID)

            });

        };
        $scope.btnPDF = function (RecId) {
            var BussData = {};
            BussData.RecID = RecId;
            $('#overlay').show();
            $('#overlay').fadeIn('slow').delay(5000).hide(0);
            $('#myIframe').attr('src', "/ReceiptPDF/ReceiptPDF?idv=" + RecId + "&AgencyID=" + $scope.HdAgencyID);

        }
      
        $scope.btnBack = function (QueryID,AgencyID) {
            
            AgencyID = $scope.HdAgencyID;
            QueryID = $scope.HdVesVoyID;
            
            var url = "/BLQuery/BLQueryDetails?QueryID=" + QueryID + "&AgencyID=" + AgencyID;
            window.location.href = url;
        }

    });
    function ShowPopup(message) {
        swal(message);
    };
</script>


