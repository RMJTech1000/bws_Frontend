<style>
    .lightgreen {
        background-color: #e6f6da;
        font-weight: 600;
    }
    .totalcolor {
        background-color: #bddea5bf;
        font-weight: bold;
        color: red;
    }
    .lightblue {
        background-color: #e0f7fd;
        font-weight: 600;
    }
    .totalbluecolor {
        background-color: #c6e9f4;
        font-weight: bold;
        color: red;
    }
    .totaloverall {
        background-color: #f3f3d6;
        font-weight: bold;
