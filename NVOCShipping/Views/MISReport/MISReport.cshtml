<div ng-app="MyApp" ng-controller="MyController">

    <div id="overlay" style="display:none;">
        <div class="loadcontainer">
            <div class="spinner"></div>
            <p>Loading Please Wait</p>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12 backbtn" style="text-align:right;">
            <a href="/MISReport/MISReportView" type="submit" class="btn btn-fill btnback btn-success btn-round btntop">
                <i class="material-icons">reply</i> Back
            </a>

        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <div class="row" style="border: 1px solid #ccc;">
                        <div class="col-md-2" style="text-align:center;">
                            <img src="~/assets/img/misrepimg.jpg" style="width:100px;" />
                        </div>
                        <div class="col-md-8" style="text-align:center;">
                            <h4 style="font-size:24px; font-weight:bold; color:#000!important; margin-top:39px;">BLUE WAVE SHIPPING MIS REPORT</h4>
                        </div>
                        <div class="col-md-2" style="text-align:center;">
                            <img src="~/assets/img/misrepimg.jpg" style="width:100px;" />
                        </div>
                    </div>


                    <div class="row" style="border: 1px solid #ccc;">
                        <div class="col-md-5">
                            <div class="col-md-12" style="margin-top:17px;">
                                <div class="row">
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-4"><label>Vessel/Voyage</label></div>
                                            <div class="col-md-8">
                                                <select id="ddlVesVoy" ng-model="ddlVesVoy" class="form-control my-select" ng-options="item.VesVoy for item in VesselNameAllvalues track by item.ID"></select>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-md-4"><label>Slot Operator</label></div>
                                            <div class="col-md-8">
                                                <select id="ddlslot" ng-model="ddlslot" class="form-control my-select" ng-options="item.VesOperator for item in SlotOperatorAllvalues track by item.ID"></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="row" style="margin-top:55px;">
                                <div class="col-md-12">
                                    
                                    <span>
                                        MIS Exchange Rate:
                                        <a href="#" ng-click="OpenMisExChageForm();">
                                            <span ng-bind="BindCurrentDate"></span>
                                        </a>
                                    </span>

                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-12" style="margin-top:28px;">
                                        <button type="submit" ng-click="btnSendEmailAlertDisplay();" class="btn btn-default" style="padding: 6px 7px;">
                                            <i class="material-icons">email</i> Send Email
                                        </button>
                                        <div class="dropdown">
                                            <a href="#" class="btn btn-primary dropdown-toggle" style="padding:6px 14px;" data-toggle="dropdown">
                                                Download
                                            </a>
                                            <div class="dropdown-menu">
                                                <a class="dropdown-item" href="#" ng-click="BtnPDFOpen();"><i class="fa fa-download" style="color: #0c92d5; font-size:15px;"></i> <span style="padding-left:15px;">XLS</span></a>
                                                <a class="dropdown-item" href="#"><i class="fa fa-envelope" style="color: #0c92d5; font-size: 15px;"></i> <span style="padding-left:15px;">PDF</span></a>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>



                    <div class="row" style="border: 1px solid #ccc; margin-top:15px;">
                        <div class="col-md-12 padlr0 view" style="padding:7px;">
                            <div class="material-datatables wrapper">
                                <table class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th class="sticky-col first-col">S.No</th>
                                            <th class="sticky-col second-col">Rate Request No</th>
                                            <th class="sticky-col third-col">BLNo</th>
                                            <th class="sticky-col four-col">Container No</th>


                                            <th>POL</th>

                                            <th>POD</th>
                                            <th>T/S</th>
                                            <th>FPOD</th>
                                            <th>Vessel/Voyage</th>
                                            <th>Terminal Name</th>
                                            <th>Slot Operator</th>
                                            <th>ETD</th>
                                            <th>Commodity Type</th>
                                            <th>20</th>
                                            <th>40</th>
                                            <th>Container Types</th>
                                            @*<th>20'RF'</th>
                                                <th>40'RF'</th>*@
                                            <th>Lease Type</th>
                                            <th colspan="1">Ocen Freight ($) </th>
                                            <th colspan="1">BAF ($)</th>
                                            <th colspan="1">Surcharges ($)</th>

                                            <th colspan="1">Load Port THC ($)</th>
                                            <th colspan="1">Discharge Port THC ($)</th>
                                            <th>HDL/TPT ($)</th>
                                            <th colspan="1">MISC ($)</th>
                                            <th>Total Revenue ($)</th>
                                            <th colspan="1">SLOT ($)</th>
                                            <th colspan="1">T/S THC COST ($)</th>
                                            <th colspan="1">T/S COMMISSION ($)</th>
                                            <th>T/S SLOT COST ($)</th>
                                            <th colspan="1">LOAD PORT PHC ($)</th>
                                            <th colspan="1">DISCHARGE PORT PHC ($)</th>
                                            <th colspan="1">LOAD COMMISSION ($)</th>
                                            <th colspan="1">DISCHARGE COMMISSION ($)</th>
                                            @*<th>BROK</th>
                                                <th>CFS MOVEMENT</th>*@
                                            <th colspan="1">MISC COST ($)</th>
                                            <th>LOG. FEE ($)</th>
                                            <th>TOTAL COST ($)</th>
                                            <th>PROFIT / LOSS ($)</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="gRow in viewData">
                                            <td class="sticky-col first-col">1</td>
                                            <td class="sticky-col second-col"><a href="#" ng-click="BtnRRPDFOpen(gRow.RRID)">{{gRow.RRNo}}</a></td>
                                            <td class="sticky-col third-col"><a href="#">{{gRow.BookingNo}}</a></td>
                                            <td class="sticky-col four-col">{{gRow.CntrNo}}</td>

                                            <td>{{gRow.POL}}</td>

                                            <td>{{gRow.POD}}</td>
                                            <td>{{gRow.TSPORT}}</td>
                                            <td>{{gRow.FPOD}}</td>
                                            <td>{{gRow.VesVoy}}</td>
                                            <td>{{gRow.TerminalName}}</td>
                                            <td>{{gRow.SlotOperator}}</td>
                                            <td>{{gRow.ETDDate}}</td>
                                            <td>{{gRow.Commodity}}</td>
                                            <td>{{gRow.Size20}}</td>
                                            <td>{{gRow.Size40}}</td>
                                            <td>{{gRow.TypeSize}}</td>
                                            @*<td>{{gRow.SizeRF40}}</td>
                                                <td>{{gRow.SizeOT40}}</td>*@
                                            <td>{{gRow.LeasTerms}}</td>
                                            <td class="lightgreen amtrgt">{{gRow.OFT}}</td>
                                            @*<td class="lightgreen">{{gRow.OFTCURR}}</td>*@

                                            <td class="lightgreen amtrgt">{{gRow.BAF}}</td>
                                            @*<td class="lightgreen">{{gRow.BAFCURR}}</td>*@

                                            <td class="lightgreen amtrgt"><a href="#"  ng-click="BtnRRSurchargesOpen(gRow.RRID)">{{gRow.SURCHARGES}}</a></td>

                                            <td class="lightgreen amtrgt">{{gRow.LOADTHC}}</td>


                                            <td class="lightgreen amtrgt">{{gRow.DESTTHC}}</td>


                                            <td class="lightgreen amtrgt">{{gRow.HLDTPT}}</td>
                                            <td class="lightgreen amtrgt"><a href="#"  ng-click="BtnRRRevenuMISCOpen(gRow.RRID)">{{gRow.MISC}}</a></td>
                                            <td class="totalcolor amtrgt">{{gRow.RevenuAmt}}</td>
                                            <td class="lightblue amtrgt">{{gRow.SlotAmt}}</td>

                                            <td class="lightblue amtrgt">{{gRow.TSCOST}}</td>

                                            <td class="lightblue amtrgt">{{gRow.TSCOMM}}</td>
                                            <td class="lightblue amtrgt"></td>
                                            <td class="lightblue amtrgt">{{gRow.LOADPHC}}</td>

                                            <td class="lightblue amtrgt">{{gRow.DESTPHC}}</td>

                                            <td class="lightblue amtrgt">{{gRow.EXCOMM}}</td>

                                            <td class="lightblue amtrgt">{{gRow.ICOMM}}</td>

                                            <td class="lightblue amtrgt">{{gRow.MISCCOST}}</td>


                                            <td class="lightblue amtrgt">{{gRow.LOGCOST}}</td>
                                            <td class="totalbluecolor amtrgt">{{gRow.COSTAmt}}</td>
                                            <td class="totaloverall amtrgt">{{gRow.ProfitAmt}}</td>
                                        </tr>

                                    </tbody>

                                </table>


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="demoModalEmail" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog" style="width:650px;">
            <div class="modal-content">
                <div class="modal-header" style="padding: 5px 24px 0;background-color: #4f2576; ">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <div class="row">
                        <div class="col-md-12" style="text-align:center;">
                            <h1 class="card-title" style="width: 600px; font-size: 17px; color: #fff; padding: 10px; text-align: center; font-weight:bold;">Email Send</h1>
                        </div>
                    </div>
                </div>
                <div class="modal-body">

                    <div class="row">
                        <div class="col-md-2">To:</div>

                        <div class="col-md-10 padl0">
                            <div class="form-group">
                                <textarea id="txtTo" ng-model="txtTo" class="form-control" rows="2" style="margin-left:-31px; width: 537px;"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2">Cc:</div>
                        <div class="col-md-10 padl0">
                            <div class="form-group">
                                <textarea id="txtCC" ng-model="txtCC" class="form-control" rows="2" style="margin-left:-31px; width: 537px;"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2">Bcc:</div>
                        <div class="col-md-10 padl0">
                            <div class="form-group">
                                <textarea id="txtBCC" ng-model="txtBCC" class="form-control" rows="2" style="margin-left:-31px; width: 537px;"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2">Message:</div>
