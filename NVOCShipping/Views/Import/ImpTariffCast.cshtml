                                    </thead>

                                    <tbody>
                                        <tr ng-repeat="gRow in DynamicGridTermainal">
                                            <td>
                                                <input type="checkbox" class="form-check" value="1" ng-click="OnclickCheckTerminal(gRow);" />
                                            </td>
                                            <td>{{$index+1}}</td>

                                            <td>{{gRow.CntrTypes}}</td>
                                            <td>{{gRow.Qty}}</td>
                                            <td>{{gRow.ChargeCodeID}}</td>
                                            <td>{{gRow.CurrencyID}}</td>

                                            <td class="amtrgt">{{gRow.CustomerRate}}</td>
                                            <td>{{gRow.VendorName}}</td>
                                            <td>{{gRow.Invoice}}</td>
