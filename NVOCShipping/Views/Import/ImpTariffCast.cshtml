                                                <input type="checkbox" class="form-check" ng-click="OnclickCheckLocalCost(gRow);" />
                                            </td>
                                            <td>{{$index+1}}</td>

                                            <td>{{gRow.CntrTypes}}</td>
                                            <td>{{gRow.Qty}} </td>
                                            <td>{{gRow.ChargeCodeID}}</td>
                                            <td>{{gRow.CurrencyID}}</td>

                                            <td class="amtrgt">{{gRow.CustomerRate}}</td>
                                            <td>{{gRow.VendorName}}</td>
                                            <td>{{gRow.Invoice}}</td>
                                        </tr>

                                    </tbody>
                                </table>

                            </div>
                        </div>



                        <div class="col-md-12">
                            <hl>COMMISSIONS</hl>
                        </div>

