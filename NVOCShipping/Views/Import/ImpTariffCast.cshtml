                            <div class="material-datatables">

                                <table class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th>Flag</th>
                                            <th>SNo</th>

                                            <th>Equip Type</th>
                                            <th>Qty</th>
                                            <th>Charge Description</th>
                                            <th>Currency</th>

                                            <th>Amount</th>
                                            <td>Vendor Name</td>
                                            <th>Invoice No</th>


