@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">

    LayoutApp.controller('MyController', function ($scope, $http, $timeout) {

        $('#overlay').show();
          $(document).ready(function () {

            var queryString = new Array();
            if (window.location.search.split('?').length > 1) {
                var params = window.location.search.split('?')[1].split('&');
                for (var i = 0; i < params.length; i++) {
                    var key = params[i].split('=')[0];
                    var value = decodeURIComponent(params[i].split('=')[1]);
                    queryString[key] = value;
                }
            }
            if (queryString["ImpReg"] != null) {

                $scope.RegId = queryString["ImpReg"].toString();
                $scope.FillExistingFreightTraiffCharges();
                $scope.FillExistingTerminalTraiffCharges();
                $scope.FillExistingDestinationTraiffCharges();
              
              }
        });

        $scope.DynamicGridFreight = [{ RCID: "", CntrTypes: "", ChargeCodeID: "", CurrencyID: "", PaymentModeID: "", ReqRate: "", ManifRate: "", CustomerRate: "", RateDiff: "" }];
        $scope.DynamicGridDestination = [{ RCID: "", CntrTypes: "", ChargeCodeID: "", CurrencyID: "", PaymentModeID: "", ReqRate: "", ManifRate: "", CustomerRate: "", RateDiff: "" }];
        $scope.DynamicGridTermainal = [{ RCID: "", CntrTypes: "", ChargeCodeID: "", CurrencyID: "", PaymentModeID: "", ReqRate: "", ManifRate: "", CustomerRate: "", RateDiff: "" }];
        $scope.FillExistingFreightTraiffCharges = function () {
            $('#overlay').show();
            var Datavalue = {};

            Datavalue.TraiffRegular = 135;
            Datavalue.ID = $scope.RegId;
            Datavalue.PaymentModeID = 19;
            $http({
                method: 'POST',
                url: '/api/ImportApi/ImpDestination',
                data: JSON.stringify(Datavalue),
            }).then(function (result) {
                $timeout(function () {

                    if (result.data.length > 0) {

                        if (result.data[0].ChargeCodeID != "") {
                            $scope.DynamicGridFreight.splice(0, 1);
                        }

                        angular.forEach(result.data, function (value, key) {

                            $scope.DynamicGridFreight.push({
                                'RCID': result.data[key].RCID,
                                'CntrTypes': result.data[key].CntrTypes,
                                'ChargeCodeID': result.data[key].ChargeCodeID,
                                'CurrencyID': result.data[key].CurrencyID,
                                'PaymentModeID': result.data[key].PaymentModeID,
                                'ReqRate': result.data[key].ReqRate,
                                'ManifRate': result.data[key].ManifRate,
                                'CustomerRate': result.data[key].CustomerRate,
                                'RateDiff': result.data[key].RateDiff,

                            });

                        });
                        $('#overlay').hide();
                    }
                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }

        $scope.FillExistingTerminalTraiffCharges = function () {

            $('#overlay').show();
            var Datavalue = {};
            Datavalue.TraiffRegular = 136;
            Datavalue.ID = $scope.RegId;
            Datavalue.PaymentModeID = 19;
            $http({
                method: 'POST',
                url: '/api/ImportApi/ImpDestination',
                data: JSON.stringify(Datavalue),
            }).then(function (result) {
                $timeout(function () {

                    if (result.data.length > 0) {

                        if (result.data[0].ChargeCodeID != "") {
                            $scope.DynamicGridTermainal.splice(0, 1);
                        }

                        angular.forEach(result.data, function (value, key) {

                            $scope.DynamicGridTermainal.push({
                                'RCID': result.data[key].RCID,
                                'CntrTypes':  result.data[key].CntrTypes,
                                'ChargeCodeID': result.data[key].ChargeCodeID,
                                'CurrencyID':  result.data[key].CurrencyID,
                                'PaymentModeID': result.data[key].PaymentModeID,
                                'ReqRate': result.data[key].ReqRate,
                                'ManifRate': result.data[key].ManifRate,
                                'CustomerRate': result.data[key].CustomerRate,
                                'RateDiff': result.data[key].RateDiff,

                            });

                        });
                        $('#overlay').hide();
                    }
                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }


        $scope.FillExistingDestinationTraiffCharges = function () {
            $('#overlay').show();
            var Datavalue = {};
            Datavalue.TraiffRegular = 138;
            Datavalue.ID = $scope.RegId;
            Datavalue.PaymentModeID = 19;
            $http({
                method: 'POST',
                url: '/api/ImportApi/ImpDestination',
                data: JSON.stringify(Datavalue),
            }).then(function (result) {
                $timeout(function () {

                    if (result.data.length > 0) {

                        if (result.data[0].ChargeCodeID != "") {
                            $scope.DynamicGridDestination.splice(0, 1);
                        }

                        angular.forEach(result.data, function (value, key) {

                            $scope.DynamicGridDestination.push({
                                'RCID': result.data[key].RCID,
                                'CntrTypes': result.data[key].CntrTypes,
                                'ChargeCodeID': result.data[key].ChargeCodeID,
                                'CurrencyID': result.data[key].CurrencyID,
                                'PaymentModeID': result.data[key].PaymentModeID,
                                'ReqRate': result.data[key].ReqRate,
                                'ManifRate': result.data[key].ManifRate,
                                'CustomerRate': result.data[key].CustomerRate,
                                'RateDiff': result.data[key].RateDiff,

                            });

                        });
                        $('#overlay').hide();
                    }
                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }

        $scope.getPaging = function (tab) {


            var myp = $scope.RegId;
            if (tab == 1) {
                if (myp != "0") {
                    var url = "/Import/ShipmentDetails?ImpReg=" + encodeURIComponent(myp);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 2) {
                if (myp != "0") {
                    var url = "/Import/ContainerDetails?ImpReg=" + encodeURIComponent(myp);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 3) {
                if (myp != "0") {
                    var url = "/Import/ImpDestinationCharges?ImpReg=" + encodeURIComponent(myp);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 4) {
                if (myp != "0") {
                    var url = "/Import/CAN?ImpReg=" + encodeURIComponent(myp);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 5) {
                if (myp != "0") {
                    var url = "/Import/DeliveryOrder?ImpReg=" + encodeURIComponent(myp);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 6) {
                if (myp != "0") {
                    var url = "/Import/CustomsReports?ImpReg=" + encodeURIComponent(myp);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 7) {
                if (myp != "0") {
                    var url = "/Import/DetDem?ImpReg=" + encodeURIComponent(myp);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            return false;
        }

    });
    function ShowPopup(message) {
        swal(message);
    };
</script>
