@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">

    LayoutApp.controller('MyController', function ($scope, $http, $timeout) {

        $scope.BasicBindValues = [
            { 'name': '--select--', 'ID': 0 },
            { 'name': 'BL', 'ID': 1 },
            { 'name': 'Container ', 'ID': 2 }
        ];

        $scope.PaymentTermsBindValues = [
            { 'name': '--select--', 'ID': 0 },
            { 'name': 'Collect', 'ID': 19 },
            { 'name': 'Prepaid ', 'ID': 18 }
        ];

        $scope.displayFetchTariff = false;
        $(document).ready(function () {

            var queryString = new Array();
            if (window.location.search.split('?').length > 1) {
                var params = window.location.search.split('?')[1].split('&');
                for (var i = 0; i < params.length; i++) {
                    var key = params[i].split('=')[0];
                    var value = decodeURIComponent(params[i].split('=')[1]);
                    queryString[key] = value;
                }
            }
            if (queryString["BkgID"] != null) {

                $scope.HDBkgID = queryString["BkgID"].toString();
                $scope.HDBLId = queryString["ImpReg"].toString();
                $scope.BLOffLineCheck(queryString["ImpReg"].toString());
                $scope.FillExistingTraiffCharges();
                //$scope.FillExistingTerminalTraiffCharges();
                //$scope.FillExistingHalageTraiffCharges();
                //$scope.FillExistingLocalTraiffCharges();

            }
        });


        $scope.FillExistingTraiffCharges = function () {
            var Datavalue = {};

            Datavalue.BkgID = $scope.HDBkgID;
            Datavalue.BLID = $scope.HDBLId;
            Datavalue.PaymentModeID = 19;
            $http({
                method: 'POST',
                url: '/api/ExpTraffView/ExpTariffViewImp',
                data: JSON.stringify(Datavalue),
            }).then(function (result) {
                $timeout(function () {
                    $scope.DynamicGrid = result.data;


                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }



        $scope.getPaging1 = function (tab) {


            var myp = $scope.HDBkgID;
            var myp1 = $scope.HDBLId;

            // var myp1 = $scope.HDBLId;
            if (tab == 1) {
                if (myp != "0") {
                    var url = "/Import/ImpTariffview?BkgID=" + encodeURIComponent(myp) + "&ImpReg=" + encodeURIComponent(myp1);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 2) {
                if (myp != "0") {
                    var url = "/Import/ImpTariffCast?BkgID=" + encodeURIComponent(myp) + "&ImpReg=" + encodeURIComponent(myp1);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }

            return false;
        }

        $scope.getPaging = function (tab) {

            var myp = $scope.HDBLId;
            var BkgID = $scope.HDBkgID;
            if (tab == 1) {
                if (myp != "0") {
                    var url = "/Import/ShipmentDetails?ImpReg=" + encodeURIComponent(myp) + "&BkgID=" + encodeURIComponent(BkgID);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 2) {
                if (myp != "0") {
                    var url = "/Import/ContainerDetails?ImpReg=" + encodeURIComponent(myp) + "&BkgID=" + encodeURIComponent(BkgID);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 3) {
                if (myp != "0") {
                    var url = "/Import/ImpTariffview?ImpReg=" + encodeURIComponent(myp) + "&BkgID=" + encodeURIComponent(BkgID);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 4) {
                if (myp != "0") {
                    var url = "/Import/CAN?ImpReg=" + encodeURIComponent(myp) + "&BkgID=" + encodeURIComponent(BkgID);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            else if (tab == 5) {
                if (myp != "0") {
                    var url = "/Import/DeliveryOrder?ImpReg=" + encodeURIComponent(myp) + "&BkgID=" + encodeURIComponent(BkgID);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }

            else if (tab == 7) {
                if (myp != "0") {
                    var url = "/Import/DetDem?ImpReg=" + encodeURIComponent(myp) + "&BkgID=" + encodeURIComponent(BkgID);
                    window.location.href = url;
                }
                else {
                    ShowPopup("Please Create Shipment Details")
                    return false;
                }
            }
            return false;
        }

        $scope.OnClickFetchTariffRate = function () {
            var Datavalue = {};
            Datavalue.BkgID = $scope.HDBkgID;
            Datavalue.BLID = $scope.HDBLId;

            $http({
                method: 'POST',
                url: '/api/ExpTraffView/OffLineImportTariffPortCharges',
                data: JSON.stringify(Datavalue),
            }).then(function (result) {
                $timeout(function () {
                    if (result.data.length > 0) {

                        $scope.FillExistingTraiffCharges();
                        ShowPopup(result.data[0].AlertMessage);
                    }


                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }

        $scope.BLOffLineCheck = function (ImpReg) {

            var Datavalue = {};
            Datavalue.ID = ImpReg;

            $http({
                method: 'POST',
                url: '/api/ImportApi/BkgExistingDtls',
                data: JSON.stringify(Datavalue),

            }).then(function (result) {
                $timeout(function () {
                    if (result.data.length > 0) {
                        if (result.data[0].BLDirect == "2") {

                            $scope.displayFetchTariff = true
                        }

                    }

                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }


        $scope.DynamicGrid = [{ TID: "", ChargeTypeID: "", ChargeType: "", ChargeCode: "", ChargeCodeID: "", Basic: "", BasicID: "", CntrTypeID: "", CntrTypes: "", CurrencyID: "", Currency: "", Amt: "", PaymentModeID: "", PaymentMode: "", Qty: "" }];
        $scope.DynamicGrid.splice(0, 1);

        $scope.ChargeCodeAllvalues = null;
        $scope.fillChargecodeValues = function () {
            $scope.states = [];
            $http({
                method: 'POST',
                url: '/api/CommonAccessApi/ChargeCode/',
                data: {}
            }).then(function (result) {
                $timeout(function () {
                    $scope.ChargeCodeAllvalues = result.data;
                });
            }, function (result) {
                if (result.status == 500) {
                    ShowPopup("Sorry no internet connectivity Application Error.");
                }
                else if (result.status == 404) {
                    ShowPopup("Sorry no internet connectivity page not found.");
                }
                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            }).catch(function (result) {
                ShowPopup('There has been an error: ' + result);
            });
        };
        $scope.fillChargecodeValues();


        $scope.ChargeTypesAllvalues = null;
        $scope.fillChargesValues = function () {
            var Datavalue = {};
            Datavalue.ID = "40"; //GeneralMaster Table seqNo
            $http({
                method: 'POST',
                url: '/api/CommonAccessApi/ModuleValues/',
                data: JSON.stringify(Datavalue)
            }).then(function (result) {
                $timeout(function () {
                    $scope.ChargeTypesAllvalues = result.data;

                });
            }, function (result) {
                if (result.status == 500) {
                    ShowPopup("Sorry no internet connectivity Application Error.");
                }
                else if (result.status == 404) {
                    ShowPopup("Sorry no internet connectivity page not found.");
                }
                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            }).catch(function (result) {
                ShowPopup('There has been an error: ' + result);
            });
        };
        $scope.fillChargesValues();

        $scope.CntrTypesvalues = null;
        $scope.fillCntrTypeValues = function () {
            $http({
                method: 'POST',
                url: '/api/CommonAccessApi/CntrTypeValues/',
                data: {}
            }).then(function (result) {
                $timeout(function () {
                    $scope.CntrTypesvalues = result.data;

                });
            }, function (result) {
                if (result.status == 500) {
                    ShowPopup("Sorry no internet connectivity Application Error.");
                }
                else if (result.status == 404) {
                    ShowPopup("Sorry no internet connectivity page not found.");
                }
                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            }).catch(function (result) {
                ShowPopup('There has been an error: ' + result);
            });
        };
        $scope.fillCntrTypeValues();

        $scope.CurrencyValue = null;
        $scope.fillCurrencyValues = function () {
            $http({
                method: 'POST',
                url: '/api/CommonAccessApi/CurrencyValues/',
                data: {}
            }).then(function (result) {
                $timeout(function () {
                    $scope.CurrencyValue = result.data;

                });
            }, function (result) {
                if (result.status == 500) {
                    ShowPopup("Sorry no internet connectivity Application Error.");
                }
                else if (result.status == 404) {
                    ShowPopup("Sorry no internet connectivity page not found.");
                }
                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            }).catch(function (result) {
                ShowPopup('There has been an error: ' + result);
            });
        };
        $scope.fillCurrencyValues();


        $scope.Add = function (DynamicGrid) {
            var validation = "";

            var ddlchargeTypes = $('#ddlchargeTypes').val();
            if (ddlchargeTypes == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select ChargeTypes</span></br>"
            }

            var ddlchargeCode = $('#ddlchargeCode').val();
            if (ddlchargeCode == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Charge Code</span></br>"
            }
            var ddlBasic = $('#ddlBasic').val();
            if (ddlBasic == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Basic</span></br>"
            }

            var ddlBasic = $('#ddlCntrTypes').val();
            if (ddlBasic == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select CntrTypes</span></br>"
            }


            var ddlCurrency = $('#ddlCurrency').val();
            if (ddlCurrency == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Currency</span></br>"
            }
            if (validation != "") {
                ShowPopup(validation);
                return false;
            }

            var RCIDValue;
            RCIDValue = ($scope.RCID == "") ? 0 : $scope.RCID;

            var val = {
                'RCID': RCIDValue,
                'ChargeTypeID': $("#ddlchargeTypes").val(),
                'ChargeType': $("#ddlchargeTypes option:selected").text(),
                'ChargeCode': $("#ddlchargeCode option:selected").text(),
                'ChargeCodeID': $("#ddlchargeCode").val(),
                'BasicID': $("#ddlBasic").val(),
                'Basic': $("#ddlBasic option:selected").text(),
                'CntrTypeID': $("#ddlCntrTypes").val(),
                'CntrTypes': $("#ddlCntrTypes option:selected").text(),
                'CurrencyID': $("#ddlCurrency").val(),
                'Currency': $("#ddlCurrency option:selected").text(),
                'Amt': $scope.Amt,
                'Qty': $scope.Qty,
                'PaymentModeID': $("#ddlPayment").val(),
                'PaymentMode': $("#ddlPayment option:selected").text()

            };
            if ($scope.HDArrayIndex != null && $scope.HDArrayIndex.length != 0) {
                $scope.DynamicGrid[$scope.HDArrayIndex] = val;

            } else {
                $scope.DynamicGrid.push(val);
            }


            $("#ddlchargeTypes").val("");
            $("#ddlchargeCode").val("");
            $("#ddlBasic").val("");
            $("#ddlCntrTypes").val("");
            $("#ddlCurrency").val("");
            $scope.Amt = "";
            $scope.Qty = "";
            $("#ddlCountry").val("");
            $("#ddlPayment").val("");
            $scope.HDArrayIndex = "";
            $scope.TID = 0;

        }



        $scope.Remove = function (DynamicGrid, index, TID) {

            var BussData = {}
            BussData.ID = TID;
            $http({
                method: 'POST',
                url: '/api/ExpTraffView/ImportBLChargedelete/',
                data: JSON.stringify(BussData),
            }).then(function (result) {
                $timeout(function () {
                    //$scope.DynamicGrid.splice(index, TID);
                    $scope.FillExistingTraiffCharges();
                    ShowPopup(result.data[0].AlertMessage);
                });
            });



        }
        $scope.Selectvalues = function (DynamicGrid, index) {
            $scope.HDArrayIndex = index;
            $scope.RCID = $scope.DynamicGrid[index].RCID;
            $scope.ChargeType = { ID: $scope.DynamicGrid[index].ChargeTypeID };
            //$scope.functionGridselect($scope.DynamicGrid[index].ChargeTypeID);
            $scope.ChargeCodeID = { ID: $scope.DynamicGrid[index].ChargeCodeID };
            $scope.Basic = { ID: $scope.DynamicGrid[index].BasicID };
            //$scope.functionGridSelectBaisc($scope.DynamicGrid[index].BasicID);
            $scope.CntrTypes = { ID: $scope.DynamicGrid[index].CntrTypeID };
            $scope.Currency = { ID: $scope.DynamicGrid[index].CurrencyID };
            $scope.Amt = $scope.DynamicGrid[index].Amt;
            $scope.Qty = $scope.DynamicGrid[index].Qty;

            $scope.PaymentModeID = { ID: $scope.DynamicGrid[index].PaymentModeID };
        }

        $scope.OnClickTariffUpdated = function () {

            console.log($scope.DynamicGrid);
            var BussData = {};
            var Items = [];
            BussData.BkgID = $scope.HDBkgID;
            BussData.BLID = $scope.HDBLId;
            angular.forEach($scope.DynamicGrid, function (value, key) {
                Items.push('Insert:' + $scope.DynamicGrid[key].ChargeTypeID, $scope.DynamicGrid[key].CntrTypeID,
                    $scope.DynamicGrid[key].ChargeCodeID, $scope.DynamicGrid[key].BasicID,
                    $scope.DynamicGrid[key].CurrencyID, $scope.DynamicGrid[key].PaymentModeID,
                    $scope.DynamicGrid[key].Amt, $scope.DynamicGrid[key].RCID, $scope.DynamicGrid[key].Qty);
            });
            BussData.Items = Items.toString();
            $http({
                method: 'POST',
                url: '/api/ExpTraffView/UpdateCustomerCost',
                data: JSON.stringify(BussData),
            }).then(function (result) {
                ShowPopup(result.data[0].AlertMessage);
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }

    });
    function ShowPopup(message) {
        swal(message);
    };
</script>
