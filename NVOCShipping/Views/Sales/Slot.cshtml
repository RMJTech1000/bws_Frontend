            //    validation += "<span style='color:red;'>*</span> <span>Please Select Transhipment Port</span></br>"
            //}
            var Datav;
            angular.forEach(data, function (value, key) {

                Datav += data[key].ChargeCodeID.ID;

            });
            if (validation != "") {
                ShowPopup(validation);
                return false;
            }
            $('#overlay').show();


            var BussData = {}
            var Items = []
            if ($scope.RegId == "")
                BussData.ID = 0;
            else
                BussData.ID = $scope.RegId
            BussData.SlotRef = $scope.txtSLOTRef;
            BussData.ValidFrom = $("#txtFromDate").val();
           // BussData.ValidTo = $('#txtValidToDate').val()
            BussData.ServiceName = $scope.txtServiceName;
            BussData.SlotOperator = $('#ddlSlotOperator').val()
            BussData.SlotTermID = $("#ddlSlotTerm").val();
            BussData.POD = $("#ddlPortofDischarge").val();
            BussData.POL = $("#ddlPortofLoading").val();

            if ($("#ddlTranshipment").val() != null)
                BussData.TSPort = $("#ddlTranshipment").val();
            else
                BussData.TSPort = 0;


            BussData.Remarks = $scope.txtRemark;
            BussData.UserID = localStorage.getItem("UserID");
            BussData.AgentId = localStorage.getItem("AgencyID");
            BussData.SessionFinYear = "2021";
            BussData.AgentCode = "NS";
            angular.forEach(data, function (value, key)
            {
                Items.push('Insert:' + data[key].SID, data[key].ChargeCodeID.ID, data[key].Basic.ID, data[key].CntrTypes.ID, data[key].CommodityTypes.ID, data[key].Currency.ID, data[key].Amt, data[key].Agency.ID);
            });
            BussData.Itemsv = Items.toString();

            $http({
