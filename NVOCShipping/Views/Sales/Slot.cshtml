        $scope.FilSLOTExsitingValues = function (SLRegID) {
            var Datavalue = {};
            Datavalue.ID = SLRegID;

            $http({
                method: 'POST',
                url: '/api/SalesApi/SLOTExistingRecord',
                data: JSON.stringify(Datavalue),

            }).then(function (result) {
                $timeout(function ()
                {
                   
                    if (result.data[0].ChargeCodeID != "")
                    {
                        $scope.DynamicGrid.splice(0, 1);
                    }

                    angular.forEach(result.data, function (value, key) {

                       
                        $scope.DynamicGrid.push({
                            'SID': result.data[key].SID,
                            'ChargeCodeID': { ID: result.data[key].ChargeCodeID },
                            'CntrTypes': { ID: result.data[key].SizeID }, 'CommodityTypes': { ID: result.data[key].CommodityID },
                            'Currency': { ID: result.data[key].CurrencyID },
                            'Amt': result.data[key].Amount, 'Agency': { ID: result.data[key].Agent },
                            'Basic': { ID: result.data[key].Basic }
                        });

                    });

                  

                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }

