        $(document).ready(function () {

            var queryString = new Array();
            if (window.location.search.split('?').length > 1) {
                var secret_key = CryptoJS.enc.Utf8.parse(localStorage.getItem("SessionKey"));
                var iv = CryptoJS.enc.Utf8.parse(localStorage.getItem("Sessioniv"));
                var bytes = CryptoJS.AES.decrypt(window.location.search.split('?')[1].toString(), secret_key, {
                    iv: iv,
                    mode: CryptoJS.mode.CBC,
                    padding: CryptoJS.pad.Pkcs7
                });

                var params = bytes.toString(CryptoJS.enc.Utf8).split('&');
                //alert(params);
                for (var i = 0; i < params.length; i++) {
                    var key = params[i].split('=')[0];
                    var value = decodeURIComponent(params[i].split('=')[1]);
                    queryString[key] = value;

                }
            }
            if (queryString["PTRegNo"] != null) {

                $scope.RegId = queryString["PTRegNo"].toString();
                $scope.FilPortTariffExsitingMaster(queryString["PTRegNo"].toString());
                $scope.FilPortTariffExsitingValues(queryString["PTRegNo"].toString());
                document.getElementById("btnCopyTraiff").disabled = false;
                document.getElementById("btnCopyTraiff").style.cursor = 'pointer';
            }
            else {
                $scope.ddlModule = { ID: 15 };

            }
        });


        $scope.Add = function (DynamicGrid) {
            var validation = "";

            var ddlchargeTypes = $('#ddlchargeTypes').val();
            if (ddlchargeTypes == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select ChargeTypes</span></br>"
            }

            var ddlchargeCode = $('#ddlchargeCode').val();
            if (ddlchargeCode == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Charge Code</span></br>"
            }
            var ddlBasic = $('#ddlBasic').val();
            if (ddlBasic == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Basic</span></br>"
            }

            var ddlBasic = $('#ddlCntrTypes').val();
            if (ddlBasic == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select CntrTypes</span></br>"
            }


            var ddlCurrency = $('#ddlCurrency').val();
            if (ddlCurrency == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Currency</span></br>"
            }
            if (validation != "") {
                ShowPopup(validation);
                return false;
            }



            var TIDValue;
            TIDValue = ($scope.TID == "") ? 0 : $scope.TID;

            var val = {
                'TID': TIDValue,
                'ChargeTypeID': $("#ddlchargeTypes").val(),
                'ChargeType': $("#ddlchargeTypes option:selected").text(),
                'ChargeCode': $("#ddlchargeCode option:selected").text(),
                'ChargeCodeID': $("#ddlchargeCode").val(),
                'BasicID': $("#ddlBasic").val(),
                'Basic': $("#ddlBasic option:selected").text(),
                'CntrTypeID': $("#ddlCntrTypes").val(),
                'CntrTypes': $("#ddlCntrTypes option:selected").text(),
                'CurrencyID': $("#ddlCurrency").val(),
                'Currency': $("#ddlCurrency option:selected").text(),
                'Amt': $scope.Amt,
                'DestCountryID': $("#ddlCountry").val(),
                'DestCountry': $("#ddlCountry option:selected").text(),
                'HandlingChargesID': $("#ddlHandlingTerms").val(),
                'HandlingCharges': $("#ddlHandlingTerms option:selected").text()

            };
            if ($scope.HDArrayIndex != null && $scope.HDArrayIndex.length != 0) {
                $scope.DynamicGrid[$scope.HDArrayIndex] = val;
            } else {
                $scope.DynamicGrid.push(val);
            }


            $("#ddlchargeTypes").val("");
            $("#ddlchargeCode").val("");
            $("#ddlBasic").val("");
            $("#ddlCntrTypes").val("");
            $("#ddlCurrency").val("");
            $scope.Amt = "";
            $("#ddlCountry").val("");
            $("#ddlHandlingTerms").val("");
            $scope.HDArrayIndex = "";
            $scope.TID = 0;

        }

