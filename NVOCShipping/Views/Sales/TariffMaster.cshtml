            $scope.Currency = { ID: $scope.DynamicGrid[index].CurrencyID };
            $scope.Amt = $scope.DynamicGrid[index].Amt;
            $scope.DestCountry = { ID: $scope.DynamicGrid[index].DestCountryID };
            $scope.ddlHandlingTerms = { ID: $scope.DynamicGrid[index].HandlingChargesID };

