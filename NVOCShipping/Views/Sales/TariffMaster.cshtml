        <div class="loadcontainer">
            <div class="spinner"></div>
            <p>Loading Please Wait</p>
        </div>
    </div>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12 backbtn" style="text-align:right;">
                <a href="/Sales/TariffView" class="btn btn-fill btn-success btnback btn-round btntop">
                    <i class="material-icons">reply</i> Back

                </a>
                @*<a href="/Sales/TariffMaster" class="btn btn-fill btn-warning btnedit btn-round btntop">
                        <i class="material-icons">edit</i>

                    </a>*@
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="card ">
                    <div class="card-header card-header-primary card-header-text">
                        <div class="card-text">
                            <h4 class="card-title">Tariff Master</h4>
                        </div>
                        <div class="row">
                            <div class="col-md-12 download" style="text-align:right;">
                                <button type="submit" id="btnCopyTraiff" ng-click="ClickCopyTraiff();" class="btn btn-fill btn-success">
                                    <i class="material-icons">
                                        content_copy
                                    </i>
                                    Copy Tariff
                                </button>

                            </div>
                            <div>

                            </div>
                        </div>
                        <div>

                        </div>
                    </div>
                    <div class="card-body ">

                        <div class="row">

                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Tariff Mode</label>
                                            <select class="form-control my-select" id="ddlTariffMode" ng-model="ddlTariffMode" options="{ allowClear: true }" ng-options="item.GeneralName for item in TariffModeAllvalues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label> Tariff Type</label>
                                            <select class="form-control" id="ddlRegTariffTypes" ng-model="ddlRegTariffTypes" options="{ allowClear: true }" ng-options="item.GeneralName for item in TariffRegTypeAllvalues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Incidental Tariff Types</label>
                                            <select class="form-control" id="ddlTariffTypes" ng-model="ddlTariffTypes" options="{ allowClear: true }" ng-options="item.GeneralName for item in TariffTypeAllvalues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>


                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Location</label>
                                            <select class="form-control my-select" id="ddlLocationPort" ng-model="ddlLocationPort" options="{ allowClear: true }" ng-options="item.PortName for item in PortAllvalues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Module</label>
                                            <select class="form-control my-select" id="ddlModule" ng-model="ddlModule" options="{ allowClear: true }" ng-options="item.GeneralName for item in ModuleAllvalues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Shipment Type</label>
                                            <select class="form-control my-select" id="ddlShipmentType" ng-model="ddlShipmentType" options="{ allowClear: true }" ng-options="item.GeneralName for item in ShipmentTypeAllvalues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>


                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Commodity Type</label>
                                            <select class="form-control my-select" id="ddlCommodityType" ng-model="ddlCommodityType" options="{ allowClear: true }" ng-options="item.GeneralName for item in CommodityAllvalues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>


                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Group Mode</label>
                                            <select class="form-control my-select" id="ddlGroup" ng-model="ddlGroup" ng-disabled="ddlGroupHid" options="{ allowClear: true }" ng-options="item.name for item in BindDGClassValues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Service Type</label>
                                            <select class="form-control my-select" id="ddlServiceType" ng-model="ddlServiceType" ng-options="item.ServiceTypesName for item in ServiceTypesValue track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>


                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Collection Mode</label>
                                            <select class="form-control my-select" id="ddlCollectionMode" ng-model="ddlCollectionMode" options="{ allowClear: true }" ng-options="item.GeneralName for item in CollectionAllvalues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>



                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Status</label>
                                            <select class="form-control my-select" id="ddlStatus" ng-model="ddlStatus" options="{ allowClear: true }" ng-options="item.name for item in StatusBindValues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Valid From</label>
                                            <input type="text" autocomplete="off" id="txtFromDate" class="form-control datepicker" ng-model="txtFromDate" class="form-control">
                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Valid Till</label>
                                            <input type="text" autocomplete="off" id="txtToDate" class="form-control datepicker" ng-model="txtToDate" class="form-control">
                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Customer Name</label>
                                            <select class="form-control my-select" id="ddlCustomerName" ng-model="ddlCustomerName" ng-options="item.CustomerName for item in CustomerMasterAllvalues track by item.ID">
                                            </select>

                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Remark</label>
                                            <textarea id="txtRemark" ng-model="txtRemark" class="form-control" rows="5"></textarea>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="row">
                            @*<div class="col-md-12 padlr0" style="text-align:right; padding-right:16px;">
                                    <a href="#" ng-click="FileUploadingWindows();" class="btn btn-fill btn-success btnuploadfile btnadd btntop" style="margin-right:0;">
                                        <i class="material-icons">upload</i> Upload
                                    </a>

                                </div>*@
                            <div class="col-md-12">
                                <div class="material-datatables">

                                    <table class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">

                                        <thead>
                                            <tr class="gridhead">
                                                <th>S.No</th>
                                                <th>Charge Types</th>
                                                <th>Charge Code</th>
                                                <th>Basis</th>
                                                <th>Container Type</th>
                                                <th>Currency</th>
                                                <th>Amount</th>
                                                <th>Dest.Port</th>
                                                <th>Handling Terms</th>
                                                <th class="td-actions text-right">Actions</th>
                                            </tr>
                                        </thead>
                                        <tbody style="margin-top:20px!important;">
                                            <tr class="gridbody">
                                                <td></td>
                                                <td class="chg">
                                                    <input type="hidden" id="HDIDv" class="form-control" ng-model="TID" />
                                                    <input type="hidden" id="HDArrayIndex" class="form-control" ng-model="HDArrayIndex" />
                                                    <select class="form-control" id="ddlchargeTypes" ng-model="ChargeType" ng-options="item.ChargeType for item in ChargeTypesBindValues track by item.ID"></select>

                                                </td>
                                                <td class="chgcode">

                                                    <select class="form-control" id="ddlchargeCode" ng-model="ChargeCodeID"
                                                            ng-options="item.ChargeCode for item in ChargeCodeAllvalues track by item.ID">
                                                    </select>
                                                </td>
                                                <td class="basis">
                                                    <select class="form-control" id="ddlBasic" ng-model="Basic" ng-change="ContainerTypesBind();" ng-options="item.name for item in BasicBindValues track by item.ID">
                                                        
                                                    </select>
                                                </td>
                                                <td class="cont">
                                                    <select class="form-control" id="ddlCntrTypes" ng-model="CntrTypes" ng-options="item.CntrTypes for item in CntrTypesvalues track by item.ID">
                                                    </select>
                                                </td>

                                                <td class="curr">
                                                    <select class="form-control" id="ddlCurrency" ng-model="Currency" ng-options="item.Currency for item in CurrencyValue track by item.ID">
                                                    </select>
                                                </td>
                                                <td><input type="text" id="txtAmt" class="form-control amtrgt" ng-model="Amt" /></td>
                                                <td class="des">
                                                    <select class="form-control" id="ddlCountry" ng-model="DestCountry" ng-options="item.PortName for item in PortAllvalues track by item.ID">
                                                    </select>
                                                </td>

                                                <td class="des">
                                                    <select class="form-control" id="ddlHandlingTerms" ng-disabled="DDLHandlingTerms" ng-model="ddlHandlingTerms" ng-options="item.SlotTerm for item in AllHandlingTermsvalues track by item.ID">
                                                        <option value=""></option>
                                                    </select>
                                                </td>




                                                <td class="td-actions text-right entrygrid">
                                                    <button type="button" rel="tooltip" ng-click="Add(gRow)" class="btn btn-primary btn-round" data-original-title="" title="">
                                                        <i class="material-icons">add</i>
                                                    </button>

                                                </td>
                                            </tr>
                                        </tbody>

                                        <thead>
                                            <tr class="gridhead1">
                                                <th>S.No.</th>
                                                <th>Charge Types</th>
                                                <th class="wid200">Charge Code</th>
                                                <th>Basis</th>
                                                <th>Container Type</th>

                                                <th>Currency</th>
                                                <th class="wid90">Amount</th>
                                                <th>Dest.Port</th>
                                                <th>Handling Charges</th>
                                                <th class="disabled-sorting text-right">Actions</th>
                                            </tr>
                                        </thead>

                                        <tbody>


                                            <tr class="gridbody1" ng-repeat="gRow in DynamicGrid">
                                                <td>{{$index+1}}</td>

                                                <td>
                                                    {{gRow.ChargeType}}
                                                    @*<input type="text" id="txtChargeType" class="form-control" ng-model="gRow.ChargeType" />*@
                                                    <input type="hidden" id="HDChargeTypeID" class="form-control" ng-model="gRow.ChargeTypeID" />
                                                </td>
                                                <td>
                                                    {{gRow.ChargeCode}}
                                                    @*<input type="text" id="txtChargeCode" class="form-control" ng-model="gRow.ChargeCode" />*@
                                                    <input type="hidden" id="HDChargeCodeID" class="form-control" ng-model="gRow.ChargeCodeID" />
                                                </td>

                                                <td>
                                                    {{gRow.Basic}}
                                                    @*<input type="text" id="txtBasic" class="form-control" ng-model="gRow.Basic" />*@
                                                    <input type="hidden" id="HDBasicID" class="form-control" ng-model="gRow.BasicID" />
                                                </td>

                                                <td>
                                                    {{gRow.CntrTypes}}
                                                    @*<input type="text" id="txtCntrTypes" class="form-control" ng-model="gRow.CntrTypes" />*@
                                                    <input type="hidden" id="HDCntrTypeID" class="form-control" ng-model="gRow.CntrTypeID" />
                                                </td>

                                                <td>
                                                    {{gRow.Currency}}
                                                    @*<input type="text" id="txtCurrency" class="form-control" ng-model="gRow.Currency" />*@
                                                    <input type="hidden" id="HDCurrencyID" class="form-control" ng-model="gRow.CurrencyID" />
                                                </td>
                                                <td class="txtright">
                                                    {{gRow.Amt}}
                                                    @*<input type="text" id="txtAmt" class="form-control" ng-model="gRow.Amt" />*@
                                                </td>
                                                <td>
                                                    {{gRow.DestCountry}}
                                                    @*<input type="text" id="txtDestCountry" class="form-control" ng-model="gRow.DestCountry" />*@
                                                    <input type="hidden" id="HDDestCountryID" class="form-control" ng-model="gRow.DestCountryID" />
                                                </td>
                                                <td>
                                                    {{gRow.HandlingCharges}}
                                                    <input type="hidden" id="HDHandlingCharges" class="form-control" ng-model="gRow.HandlingChargesID" />
                                                </td>
                                                <td class="td-actions text-right bindgrid">


                                                    <button type="button" rel="tooltip" ng-show="gRow.ChargeTypeID ==2" ng-click="SelectTariffView(DynamicGrid,$index,gRow.TID)" class="btn btn-primary btn-round" data-original-title="" title="">
                                                        <i class="material-icons"></i>Breakup
                                                    </button>

                                                    <button type="button" rel="tooltip" ng-click="Selectvalues(DynamicGrid,$index)" class="btn btn-success btn-round" data-original-title="" title="">
                                                        <i class="material-icons">edit</i>
                                                    </button>
                                                    <button type="button" rel="tooltip" ng-click="Remove(DynamicGrid,$index,gRow.TID)" class="btn btn-danger btn-round" data-original-title="" title="">
                                                        <i class="material-icons">delete</i>
                                                    </button>
                                                    <input type="hidden" id="HDID" class="form-control" ng-model="gRow.TID" />
                                                </td>

                                            </tr>


                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="card-footer ">
                        <div class="col-md-12" style="text-align:right;">
                            <button type="submit" ng-click="Recordsave(DynamicGrid);" class="btn btn-fill btn-success">Submit</button>
                            <input type="hidden" id="RegId" name="RegId" />
                        </div>
                    </div>
                </div>


                <div class="modal fade" id="demoModalTariffView" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                    <div class="modal-dialog" style="width:1020px;">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>

                            </div>
                            <div class="modal-body">
                                <div class="row popup">
                                    <div class="col-md-12" style="text-align:center;">

                                        <div class="card-header card-header-primary card-header-text" style=" margin-bottom: 20px;">
                                            <div class="card-text">
                                                <h4 class="card-title" style="font-weight:bold;">THC COSTINGS</h4>
                                            </div>
                                        </div>

                                        <div class="col-md-12">
                                            <div class="material-datatables">

                                                <table class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">
                                                    <thead>
                                                        <tr>
                                                            <th>SNo</th>
                                                            <th>Charges</th>
                                                            <th>Shipment Type</th>
                                                            <th>Commodity</th>
                                                            <th>Cntr Type</th>
                                                            <th>Currency</th>
                                                            <th>Amount</th>

                                                            <th class="disabled-sorting text-right actwid">Actions</th>
                                                        </tr>
                                                    </thead>

                                                    <tbody>
                                                        <tr ng-repeat="gRow in DynamicGridMUCCharges">
                                                            <td>{{$index+1}}</td>


                                                            <td>
                                                                <select class="form-control" id="ddlchargeCode" ng-model="gRow.ChargeCodeID" ng-options="item.ChargeCode for item in ChargeDescription track by item.ID"></select>
                                                            </td>
                                                            <td>
                                                                <select class="form-control" id="ddlshipmentTypes" ng-model="gRow.ShipmentTypeID" ng-options="item.GeneralName for item in FillShipmentTypes track by item.ID"></select>
                                                            </td>

                                                            <td>
                                                                <select class="form-control" id="ddlCommodity" ng-model="gRow.CommodityTypeID" ng-options="item.GeneralName for item in CommodityAllvalues track by item.ID"></select>
                                                            </td>

                                                            <td>
                                                                <select class="form-control" id="ddlCntrTypes" ng-model="gRow.CntrTypeID" ng-options="item.CntrTypes for item in CntrTypesvalues track by item.ID"></select>
                                                            </td>
                                                            <td>
                                                                <select class="form-control" id="ddlCurrency" ng-model="gRow.CurrencyID" ng-options="item.Currency for item in CurrencyValue track by item.ID">
                                                                </select>
                                                            </td>

                                                            <td><input type="text" id="txtReqRate" autocomplete="off" class="form-control" ng-change="BindTotalMSCCharges(DynamicGridMUCCharges);" ng-model="gRow.Amount" /></td>

                                                            <td class="td-actions text-right actwid">
                                                                <button type="button" rel="tooltip" ng-if="$last" ng-click="MUCChargesAdd(gRow)" class="btn btn-primary btn-round" data-original-title="" title="">
                                                                    <i class="material-icons">add</i>
                                                                </button>
                                                                <button type="button" rel="tooltip" ng-if="!$last" ng-click="MUCChargesDelete(DynamicGridMUCCharges,$index,gRow.TCID)"  class="btn btn-danger btn-round" data-original-title="" title="">
                                                                    <i class="material-icons">delete</i>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td colspan="6"></td>
                                                            <td><span ng-bind="TotalMCHCharges"> </span> </td>
                                                        </tr>
                                                    </tbody>
                                                </table>

                                            </div>



                                        </div>

                                        <div class="col-md-12">

                                            <button type="submit" id="btnudate" ng-click="OnclickTariffDetailUpdate(DynamicGridMUCCharges);" class="btn btn-fill btn-primary btnupdate btntop">
                                                <i class="material-icons"></i>
                                                Update
                                            </button>

                                        </div>
                                    </div>



                                </div>
                            </div>




                        </div>
                    </div>
                </div>



            </div>

        </div>

    </div>
</div>
<script>
    $(document).ready(function () {
        $('.datepicker').datetimepicker({
            format: 'DD/MM/YYYY',
        });
    });
</script>

