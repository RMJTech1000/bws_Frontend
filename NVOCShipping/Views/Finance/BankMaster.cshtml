<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.full.min.js"></script>
<script type="text/javascript">

    LayoutApp.controller('MyController', function ($scope, $http, Upload, $timeout) {
        $scope.HDArrayIndex = "";
        $(document).ready(function () {
            $('.my-select').select2({
                placeholder: "--Select--",
                allowClear: true,
                width: '100%'
            });

        });
        $scope.StatusValues = [
            { 'name': 'ACTIVE', 'ID': 1 },
            { 'name': 'INACTIVE ', 'ID': 0 }
        ];
        $scope.BankMasterModeList = null;
        $scope.FillBankMasterModeValues = function () {
            $http({

                method: 'POST',
                url: '/api/FinanceApi/BankModeValues/',
                data: {}
            }).then(function (result) {
                $timeout(function () {
                    $scope.BankMasterModeList = result.data;

                });
            }, function (result) {
                if (result.status == 500) {
                    ShowPopup("Sorry no internet connectivity Application Error.");
                }
                else if (result.status == 404) {
                    ShowPopup("Sorry no internet connectivity page not found.");
                }
                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            }).catch(function (result) {
                ShowPopup('There has been an error: ' + result);
            });

        };
        $scope.FillBankMasterModeValues();

        $scope.CurrencyValuesList = null;
        $scope.FillCurrencyValues = function () {
            $http({

                method: 'POST',
                url: '/api/CommonAccessApi/CurrencyValues/',
                data: {}
            }).then(function (result) {
                $timeout(function () {
                    $scope.CurrencyValuesList = result.data;

                });
            }, function (result) {
                if (result.status == 500) {
                    ShowPopup("Sorry no internet connectivity Application Error.");
                }
                else if (result.status == 404) {
                    ShowPopup("Sorry no internet connectivity page not found.");
                }
                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            }).catch(function (result) {
                ShowPopup('There has been an error: ' + result);
            });

        };
        $scope.FillCurrencyValues();

        $scope.GLCodeValues = null;
        $scope.fillGLCodeValues = function () {
            $http({
                method: 'POST',
                url: '/api/FinanceApi/GLCodeMasterRecord/',
                data: {}
            }).then(function (result) {
                $timeout(function () {
                    $scope.GLCodeValues = result.data;

                });
            }, function (result) {
                if (result.status == 500) {
                    ShowPopup("Sorry no internet connectivity Application Error.");
                }
                else if (result.status == 404) {
                    ShowPopup("Sorry no internet connectivity page not found.");
                }
                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            }).catch(function (result) {
                ShowPopup('There has been an error: ' + result);
            });
        };
        $scope.fillGLCodeValues();

        $scope.AgencyMasterList = null;
        $scope.FilAgencyMasterList = function () {
            $http({
                method: 'POST',
                url: '/api/CommonAccessApi/AgencyMaster/',
                data: {}
            }).then(function (result) {
                $timeout(function () {
                    $scope.AgencyMasterList = result.data;

                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected. Please reconnect and try again.");
                }
            });

        };
        $scope.FilAgencyMasterList();


        $(document).ready(function () {

            var queryString = new Array();
            if (window.location.search.split('?').length > 1) {
                var secret_key = CryptoJS.enc.Utf8.parse(localStorage.getItem("SessionKey"));
                var iv = CryptoJS.enc.Utf8.parse(localStorage.getItem("Sessioniv"));
                var bytes = CryptoJS.AES.decrypt(window.location.search.split('?')[1].toString(), secret_key, {
                    iv: iv,
                    mode: CryptoJS.mode.CBC,
                    padding: CryptoJS.pad.Pkcs7
                });

                var params = bytes.toString(CryptoJS.enc.Utf8).split('&');
                //alert(params);
                for (var i = 0; i < params.length; i++) {
                    var key = params[i].split('=')[0];
                    var value = decodeURIComponent(params[i].split('=')[1]);
                    queryString[key] = value;

                }
            }
            if (queryString["BMID"] != null) {

                $scope.HdRegId = queryString["BMID"].toString();
                $scope.BindBankMaster(queryString["BMID"].toString());
                $scope.BindBankMasterDtls(queryString["BMID"].toString());
                $scope.BindBankMasterAttachDtls(queryString["BMID"].toString());
            }


        });


        $scope.BindBankMaster = function (BMID) {
            var Datavalue = {};
            Datavalue.ID = BMID;
            $http({
                method: 'POST',
                url: "/api/FinanceApi/BankMasterEdit",
                data: JSON.stringify(Datavalue),
            }).then(function (result) {
                $timeout(function () {
                    if (result.data.length > 0) {
                        // alert(result.data[0].TaxName);
                        $scope.txtAccNo = result.data[0].AccountNo;
                        $scope.txtBankCode = result.data[0].BankCode;
                        $scope.txtBankName = result.data[0].BankName;
                        $scope.ddlCurrency = { ID: result.data[0].CurrencyID };
                        $scope.ddlMode = { ID: result.data[0].ModeID };
                        $scope.ddlGL = { ID: result.data[0].GLCodeID };
                        $scope.ddlStatus = { ID: result.data[0].StatusID };
                        $scope.txtIFSCCode = result.data[0].IFSCCode;
                        $scope.txtSwiftCode = result.data[0].SwiftCode;
                        $scope.txtBranchAddress = result.data[0].BranchAddress;
                        $scope.txtBranchName = result.data[0].BranchName;
                    }
                    else {
                        $scope.txtAccNo = "";
                        $scope.txtBankCode = "";
                        $scope.txtBankName = "";
                        $scope.ddlCurrency = 0;
                        $scope.ddlMode = 0;
                        $scope.ddlGL = 0;
                        $scope.ddlStatus = 0;
                    }
                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected. Please reconnect and try again.");
                }
            });

        }

        $scope.BindBankMasterDtls = function (BMID) {
            var Datavalue = {};
            Datavalue.ID = BMID;

            $http({
                method: 'POST',
                url: "/api/FinanceApi/BindBankMasterDtls",
                data: JSON.stringify(Datavalue),
            }).then(function (result) {

                    $timeout(function () {
                        if (result.data.length > 0) {
                            if (result.data[0].AgencyID != "") {
                                $scope.DynamicGrid.splice(0, 1);
                            }

                        angular.forEach(result.data, function (value, key) {

                            $scope.DynamicGrid.push({
                                'BID': result.data[key].BID,
                                'AgencyID': result.data[key].AgencyID,
                                'Agency': result.data[key].Agency,

                            });

                        });
                    }
                });

            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }

        $scope.BindBankMasterAttachDtls = function (BMID) {
            var Datavalue = {};
            Datavalue.ID = BMID;

            $http({
                method: 'POST',
                url: "/api/FinanceApi/BindBankMasterAttachDtls",
                data: JSON.stringify(Datavalue),
            }).then(function (result) {


                $timeout(function () {
                    if (result.data.length > 0)
                    {

                    if (result.data[0].File != "") {
                        $scope.DynamicGridFileAttahed.splice(0, 1);
                    }

                    angular.forEach(result.data, function (value, key) {

                        $scope.DynamicGridFileAttahed.push({
                            'AID': result.data[key].AID,
                            'File': result.data[key].File,
                            'FileName': result.data[key].FileName,

                        });

                    });
                }
                });

            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }


        $scope.DynamicGrid = [{ BID: "", AgencyID: "", Agency: "" }];

        $scope.DynamicGrid.splice(0, 1);


        $scope.Add = function (DynamicGrid) {


            var BIDValue;

            BIDValue = ($scope.BID == "") ? 0 : $scope.BID;

            var val = {
                'BID': BIDValue,
                'AgencyID': $("#ddlAgency").val(),
                'Agency': $("#ddlAgency option:selected").text(),

            };
            if ($scope.HDArrayIndex != null && $scope.HDArrayIndex.length != 0) {

                $scope.DynamicGrid[$scope.HDArrayIndex] = val;
            } else {

                $scope.DynamicGrid.push(val);
            }

            $("#ddlAgency").val("");
            $scope.HDArrayIndex = "";
            $scope.BID = 0;

        }
        $scope.Remove = function (DynamicGrid, index, BID) {

            var BussData = {}
            BussData.BID = BID;
            $scope.DynamicGrid.splice(index, 1);
            $http({
                method: 'POST',
                url: '/api/FinanceApi/BankMasterDtlsDelete/',
                data: JSON.stringify(BussData),
            }).then(function (result) {
                $timeout(function () {
                    $scope.DynamicGrid.splice(index, BID);
                    // $scope.BindBankMasterDtls($scope.HdRegId);
                });
            });

        }

        $scope.Recordsave = function (data, dataV) {


            var validation = "";
            var txtBankCode = $('#txtBankCode').val();
            if (txtBankCode.length == 0) {

              validation += "<span style='color:red;'>*</span> <span>Please Enter Bank Code</span></br>"
            }
            var txtBankName = $('#txtBankName').val();
            if (txtBankName.length == 0) {

                validation += "<span style='color:red;'>*</span> <span>Please Enter Bank Name</span></br>"
            }
            var txtAccNo = $('#txtAccNo').val();
            if (txtAccNo.length == 0) {

                validation += "<span style='color:red;'>*</span> <span>Please Enter Account No</span></br>"
            }
            var ddlMode = $('#ddlMode').val();
            if (ddlMode == "?") {

                validation += "<span style='color:red;'>*</span> <span>Please Select Mode</span></br>"
            }

            var ddlCurrency = $('#ddlCurrency').val();
            if (ddlCurrency == "?") {

                validation += "<span style='color:red;'>*</span> <span>Please Select Currency</span></br>"
            }

            var ddlGL = $('#ddlGL').val();
            if (ddlGL == "?") {

                validation += "<span style='color:red;'>*</span> <span>Please Select GL </span></br>"
            }

            if (validation != "") {

                ShowPopup(validation);
                return false;
            }
            var Datav = "undefined";
            angular.forEach(data, function (value, key) {

                Datav += data[key].AgencyID;

            });

            if (Datav.toString() == "undefined") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Agency Details </span></br>"
            }

            if (validation != "") {
                ShowPopup(validation);
                return false;
            }
            $('#overlay').fadeIn().delay(100).fadeOut();


            var BussData = {}

            var id = $scope.HdRegId;

            if (id == "" || id == null)
                BussData.ID = 0;
            else
                BussData.ID = $scope.HdRegId;

            BussData.BankCode = $scope.txtBankCode;
            BussData.BankName = $scope.txtBankName;
            BussData.AccountNo = $scope.txtAccNo;
            BussData.UserID = localStorage.getItem("UserID");
            BussData.ModeID = $('#ddlMode').val();
            BussData.CurrencyID = $('#ddlCurrency').val();
            BussData.GLCodeID = $('#ddlGL').val();
            BussData.StatusID = $('#ddlStatus').val();
            BussData.IFSCCode = $scope.txtIFSCCode;
            BussData.BranchName = $scope.txtBranchName;
            BussData.SwiftCode = $scope.txtSwiftCode;
            BussData.BranchAddress = $scope.txtBranchAddress;
            var Items = []
            var ItemsDoc = []

            angular.forEach(data, function (value, key) {
                var intBID = data[key].BID;
                if (typeof data[key].BID == "undefined") {
                    intBID = 0;
                }

                Items.push('Insert:' + intBID, data[key].AgencyID, data[key].Agency
                );
            });
            angular.forEach(dataV, function (value, key) {
                var intAID = dataV[key].AID;
                if (typeof dataV[key].AID == "undefined") {
                    intAID = 0;
                }
                ItemsDoc.push('Insert:' + intAID, dataV[key].File,dataV[key].FileName

                );
            });

            BussData.ItemsDoc = ItemsDoc.toString();
            BussData.Itemsv = Items.toString();
            console.log(Items);
            $http({
                method: 'POST',
                url: "/api/FinanceApi/InsertBankMaster",
                data: JSON.stringify(BussData),
            }).then(function (result) {
                $timeout(function () {
                    if (BussData.ID != "") {
                        $('#overlay').hide;
                        ShowPopup("Bank Master Updated Successfully");
                    }
                    else {

                        ShowPopup("Bank Master Saved Successfully");
                    }
                    var url = "/Finance/BankMasterView";
                    window.location.href = url;
                    setTimeOut(function () {
                        window.location = "/";
                    }, 3000);
                });
            }, function (result) {
                if (result.status == 500) {
                    ShowPopup("Sorry no internet connectivity Application Error.");
                }
                else if (result.status == 404) {
                    ShowPopup("Sorry no internet connectivity page not found.");
                }


                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    // $(".modal").hide();
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            }).catch(function (result) {
                // This is set in the event of an error.
                console.log(result);
                ShowPopup('There has been an error: ' + result);
            });

        }
        $scope.DynamicGridFileAttahed = [{ AID: "", File: "", FileName: ""}];
        $scope.DynamicGridFileAttahed.splice(0, 1);

        $scope.AddAttach = function (DynamicGridFileAttahed) {

            var validation = "";

            //var File= $('#txtFile').val();
            //if (File.length == 0) {

            //    validation += "<span style='color:red;'>*</span> <span>Please Enter FileName</span></br>"
            //}
            //var FileName = $('#FileName').val();
            //if (FileName.length == 0) {

            //    validation += "<span style='color:red;'>*</span> <span>Please Enter Upload File</span></br>"
            //}

            if (validation != "") {
                ShowPopup(validation);
                return false;
            }


            var val = {
                'File': $scope.txtFile,
                'FileName': $scope.txtFileName,

            };
            if ($scope.HDArrayIndex != null && $scope.HDArrayIndex.length != 0) {

                $scope.DynamicGridFileAttahed[$scope.HDArrayIndex] = val;
            } else {

                $scope.DynamicGridFileAttahed.push(val);
            }

            $scope.txtFileName = "";
            $scope.txtFile = "";
            $scope.HDArrayIndex = "";
            $scope.AID = 0;

        }

        $scope.RemoveAttach = function (DynamicGridFileAttahed, index, AID) {
          
            var BussData = {}
            BussData.AID = AID;
            $scope.DynamicGridFileAttahed.splice(index, 1);
            $http({
                method: 'POST',
                url: '/api/FinanceApi/BankAttachmentsDelete/',
                data: JSON.stringify(BussData),
            }).then(function (result) {
                $timeout(function () {
                    $scope.DynamicGridFileAttahed.splice(index, AID);
                    $scope.BindBankMasterAttachDtls($scope.HdRegId);
                });
            });



        }

        $scope.UploadFiles = function (files) {
            var validation = "";
            //var txtFile = $('#txtFile').val();
            //if (txtFile == "") {
            //    validation += "<span style='color:red;'>*</span> <span>Please Enter File</span></br>"
            //}
            if (validation != "") {
                $('#upload-file-info').text("");
                ShowPopup(validation);
                return false;
            }


            $scope.SelectedFiles = files;
            if ($scope.SelectedFiles && $scope.SelectedFiles.length) {
                if ($scope.SelectedFiles && $scope.SelectedFiles.length) {
                    Upload.upload({
                        url: '/Finance/Upload/',
                        data: {
                            files: $scope.SelectedFiles

                        }
                    }).then(function (response) {
                        $timeout(function () {


                            $scope.txtFileName = response.data;
                            $('#Upload').text("");



                        });
                    }, function (response) {
                        if (response.status > 0) {
                            var errorMsg = response.status + ': ' + response.data;

                        }
                    }, function (evt) {
                        var element = angular.element(document.querySelector('#dvProgress'));
                        $scope.Progress = Math.min(100, parseInt(100.0 * evt.loaded / evt.total));
                        element.html('<div style="width: ' + $scope.Progress + '%">' + $scope.Progress + '%</div>');
                    });
                }

            }

        }
        $scope.back = function () {
            window.location = "/Finance/BankMasterView";
        }
          $scope.DownloadFile = function (fileName) {

            window.location.href = '@Url.Action("DownloadAttachment", "Finance")?fileName=' + fileName;
        };
    });

    function ShowPopup(message) {
        swal(message);
    };


</script>
