<div ng-app="MyApp" ng-controller="MyController">
    <div id="overlay" style="display:none;">
        <div class="loadcontainer">
            <div class="spinner"></div>
            <p>Loading Please Wait</p>
        </div>
    </div>

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12 backbtn" style="text-align:right;">
                <a ng-click="back();" class="btn btn-fill btn-success btn-round btntop" style="color:#fff;">
                    <i class="material-icons">reply</i> Back

                </a>
                @*<button type="submit" ng-click="AddNew();" class="btn btn-fill btn-warning btn-round btntop">
                        <i class="material-icons">edit</i>

                    </button>*@
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="card ">
                    <div class="card-header card-header-primary card-header-text">
                        <div class="card-text">
                            <h4 class="card-title"> Role Management</h4>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="row" style="border:1px solid #ccc; padding-bottom:20px;">
                            <div class="col-md-6" style="border-right:1px solid #ccc;">
                                <div class="row" style="border-bottom:1px solid #ccc;">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Role Code</label>
                                            <input type="text" id="txtRoleCode" ng-model="txtRoleCode" class="form-control" />
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Role Description</label>
                                            <input type="text" id="txtDescription" ng-model="txtDescription" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row" style="border-bottom:1px solid #ccc;">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Program Name</label>
                                            <select id="ddlProgramName" ng-model="ddlProgramName" ng-options="item.FileName for item in MenuValues track by item.ID" class="form-control my-select"></select>


                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="row" style="margin-top:41px;">
                                            <div class="col-md-3" style="text-align:center;">
                                                <input type="checkbox" id="ChkProgCreate" ng-model="ChkProgCreate" ng-true-value="1" ng-false-value="0" />
                                                <label style="padding: 0px 4px;">Create</label>
                                            </div>
                                            <div class="col-md-3" style="text-align:center;">
                                                <input type="checkbox" id="ChkProgUpdate" ng-model="ChkProgUpdate" ng-true-value="1" ng-false-value="0" />
                                                <label style="padding: 0px 4px;">Update</label>
                                            </div>
                                            <div class="col-md-3" style="text-align:center;">
                                                <input type="checkbox" id="ChkProgDelete" ng-model="ChkProgDelete" ng-true-value="1" ng-false-value="0" />
                                                <label style="padding: 0px 4px;">Delete</label>
                                            </div>
                                            <div class="col-md-3" style="text-align:center;">
                                                <input type="checkbox" id="ChkProgView" ng-model="ChkProgView" ng-true-value="1" ng-false-value="0" />
                                                <label style="padding: 0px 4px;">View</label>
                                            </div>
                                        </div>





                                    </div>


                                </div>
                                <div class="row" style="border-bottom:1px solid #ccc;">
                                    <div class="col-md-12" style="text-align:right;">
                                        <button class="btn btn-warning" ng-click="AddRecord(gRow);">ADD</button>
                                        @*<input type="hidden" id="RegId" ng-model="RegId" />
                                            <input type="hidden" id="HdId" ng-model="HdId" />*@
                                        <input type="hidden" id="HDArrayIndex" class="form-control" ng-model="HDArrayIndex" />
                                    </div>
                                </div>
                                <div class="row" style="margin-top:10px;">
                                    <div class="col-md-12">
                                        <div class="material-datatables" style="overflow:scroll;">

                                            <table class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">
                                                <thead>
                                                    <tr>
                                                        <th>Program Name</th>
                                                        <th>Create</th>
                                                        <th>Update</th>
                                                        <th>Delete</th>
                                                        <th>View</th>
                                                        <th>Action</th>

                                                    </tr>
                                                </thead>

                                                <tbody>
                                                    <tr ng-repeat="gRow in StaffRoleRec | filter : paginate | orderBy:'-Statusv'">
                                                        <td>
                                                            {{gRow.MenuName}}
                                                            <input type="hidden" id="MID" ng-model="gRow.MenuID" />
                                                        </td>
                                                        <td>
                                                            <input type="checkbox" ng-model="gRow.CreateID" ng-true-value="1" ng-false-value="0" id="Create" />

                                                        </td>
                                                        <td>
                                                            <input type="checkbox" ng-model="gRow.UpdateID" ng-true-value="1" ng-false-value="0" id="Update" />
                                                        </td>
                                                        <td>
                                                            <input type="checkbox" ng-model="gRow.DeleteID" ng-true-value="1" ng-false-value="0" id="Delete" />
                                                        </td>
                                                        <td>
                                                            <input type="checkbox" ng-model="gRow.ViewID" ng-true-value="1" ng-false-value="0" id="View" />
                                                        </td>
                                                        <td class="td-actions text-right bindgrid">
                                                            <button type="button" rel="tooltip" ng-click="Selectvalues(StaffRoleRec,$index)" class="btn btn-success btn-round" data-original-title="" title="">
                                                                <i class="material-icons">edit</i>
                                                            </button>
                                                            <button type="button" rel="tooltip" ng-click="Remove(StaffRoleRec,$index,gRow.MID)" class="btn btn-danger btn-round" data-original-title="" title="">
                                                                <i class="material-icons">delete</i>
                                                            </button>
                                                        </td>
                                                    </tr>



                                                </tbody>
                                            </table>

                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="row" style="border-bottom:1px solid #ccc;">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Module Name</label>
                                            <select id="ddlModuleName" ng-model="ddlModuleName" ng-options="item.Description for item in MenuModulValues track by item.ID" class="form-control my-select"></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="row" style="border-bottom:1px solid #ccc;">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Process Name</label>
                                            <select id="ddlProcessName" ng-model="ddlProcessName" ng-options="item.Description for item in MenuAccessValues track by item.ID" class="form-control"></select>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="row" style="margin-top:41px;">
                                            <div class="col-md-3" style="text-align:center;">
                                                <input type="checkbox" id="ChkCreate" ng-model="ChkCreate" ng-true-value="1" ng-false-value="0" />
                                                <label style="padding: 0px 4px;">Create</label>
                                            </div>
                                            <div class="col-md-3" style="text-align:center;">
                                                <input type="checkbox" id="ChkUpdate" ng-model="ChkUpdate" ng-true-value="1" ng-false-value="0" />
                                                <label style="padding: 0px 4px;">Update</label>
                                            </div>
                                            <div class="col-md-3" style="text-align:center;">
                                                <input type="checkbox" id="ChkDelete" ng-model="ChkDelete" ng-true-value="1" ng-false-value="0" />
                                                <label style="padding: 0px 4px;">Delete</label>
                                            </div>
                                            <div class="col-md-3" style="text-align:center;">
                                                <input type="checkbox" id="ChkView" ng-model="ChkView" ng-true-value="1" ng-false-value="0" />
                                                <label style="padding: 0px 4px;">View</label>
                                            </div>
                                        </div>





                                    </div>
                                </div>
                                <div class="row" style="border-bottom:1px solid #ccc;">
                                    <div class="col-md-12" style="text-align:right;">
                                        <button class="btn btn-warning" ng-click="AddRecordMenuAccess(StaffRoleSubMenu);">ADD</button>
                                        @*<input type="hidden" id="RegId" ng-model="RegId" />
                                            <input type="hidden" id="HdId" ng-model="HdId" />*@
                                        <input type="hidden" id="HDArrayIndex1" class="form-control" ng-model="HDArrayIndex1" />
                                    </div>
                                </div>
                                <div class="row" style="margin-top:10px;">
                                    <div class="col-md-12">
                                        <div class="material-datatables" style="overflow:scroll;">

                                            <table class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">
                                                <thead>
                                                    <tr>
                                                        <th>Process Name</th>
                                                        <th>Create</th>
                                                        <th>Update</th>
                                                        <th>Delete</th>
                                                        <th>View</th>
                                                        <th>Action</th>

                                                    </tr>
                                                </thead>

                                                <tbody>
                                                    <tr ng-repeat="gRow in StaffRoleSubMenu | filter : paginate | orderBy:'-Statusv'">
                                                        <td>
                                                            {{gRow.MenuName}}
                                                            <input type="hidden" id="MID" ng-model="gRow.MenuID" />
                                                        </td>
                                                        <td>
                                                            <input type="checkbox" ng-model="gRow.CreateID" ng-true-value="1" ng-false-value="0" id="Create" />
                                                        </td>
                                                        <td>
                                                            <input type="checkbox" ng-model="gRow.UpdateID" ng-true-value="1" ng-false-value="0" id="Update" />
                                                        </td>
                                                        <td>
                                                            <input type="checkbox" ng-model="gRow.DeleteID" ng-true-value="1" ng-false-value="0" id="Delete" />
