      $scope.RentSave = function (data) {
          var validation = "";


            var ddlTariffType = $('#ddlTariffType').val();
            if (ddlTariffType == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Tariff Type</span></br>"
            }
            var ddlSizeType = $('#ddlSizeType').val();
            if (ddlSizeType == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Container Type</span></br>"
          }
          var ddlChargeType = $('#ddlChargeType').val();
            if (ddlChargeType == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Charge Type</span></br>"
          }
          var ddlCharges = $('#ddlCharges').val();
            if (ddlCharges == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Charges</span></br>"
          }
           var ddlChargeOwner = $('#ddlChargeOwner').val();
            if (ddlChargeOwner == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Charge Owner</span></br>"
          }
           var ddlShipmentType = $('#ddlShipmentType').val();
            if (ddlShipmentType == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Shipment Type</span></br>"
          }
           var ddlPort = $('#ddlPort').val();
            if (ddlPort == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Port</span></br>"
          }
          var ddlCustomer = $('#ddlCustomer').val();
            if (ddlCustomer == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Customer</span></br>"
          }
          var ddlTerminal = $('#ddlTerminal').val();
            if (ddlTerminal == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Terminal</span></br>"
          }
           var ddlDepot = $('#ddlDepot').val();
            if (ddlDepot == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Depot</span></br>"
            }
            var txtDtValidFrom = $('#txtDtValidFrom').val();
            if (txtDtValidFrom.length == 0) {
                validation += "<span style='color:red;'>*</span> <span>Please Enter Valid From Date</span></br>"
            }
            var txtDtValidTill = $('#txtDtValidTill').val();
            if (txtDtValidTill.length == 0) {
                validation += "<span style='color:red;'>*</span> <span>Please Enter Valid Till Date</span></br>"
            }
            if (validation != "") {
                ShowPopup(validation);
                return false;
            }
       
            var BussData = {}
            var Items =[]

            if ($scope.HdRentID == "")
                BussData.ID = 0;
            else
          BussData.ID = $scope.HdRentID;
          BussData.TariffTypeID = $('#ddlTariffType').val();
          BussData.ContainerType = $('#ddlSizeType').val();
          BussData.ChargeTypeID = $('#ddlChargeType').val();
          BussData.ChargesID = $('#ddlCharges').val();
          BussData.ChargeOwnerID = $('#ddlChargeOwner').val();
          BussData.ShipmentTypeID = $('#ddlShipmentType').val();
          BussData.PortID = $('#ddlPort').val();
          BussData.CustomerID = $('#ddlCustomer').val();
          BussData.TerminalID = $('#ddlTerminal').val();
          BussData.DepotID = $('#ddlDepot').val();
          BussData.StartingMove = $('#ddlStartingMove').val();
          BussData.EndingMove = $('#ddlEndingMove').val();
          BussData.AutoRunTypeID = $('#ddlAutoRunType').val();
          BussData.ValidFrom = $('#txtDtValidFrom').val();
          BussData.ValidTill = $('#txtDtValidTill').val();
           if ($scope.ChkStatus == 1) {

                BussData.Status = 1;
            }
            else {
                BussData.Status = 0;
            }
            BussData.Status = $scope.ChkStatus;
          
        

            angular.forEach(data, function (value, key)
            {
               var intCID = data[key].CID;
                if (typeof data[key].CID == "undefined") {
                    intCID = 0;
                }

               

                Items.push('Insert:' + intCID, data[key].SlabFrom, data[key].SlabTo, data[key].CurrencyID, data[key].Currency, data[key].Amount);
                    
            });
            BussData.Itemsv1 = Items.toString();
            $http({
                method: 'POST',
                url: "/api/ContainerApi/ContainerRent",
                data: JSON.stringify(BussData),

            }).then(function (result) {
                $timeout(function () {
                    $scope.HdRentID = result.data[0].ID;
                   
                    if (BussData.ID != "")
                    {                       
                        ShowPopup("Customer Master Updated");
                        
                    }
                    else
                    {
                        ShowPopup("Customer Master Saved Successfully");
                    }
                });
            });

        }
         $(document).ready(function () {

            var queryString = new Array();
            if (window.location.search.split('?').length > 1) {
                var params = window.location.search.split('?')[1].split('&');
                for (var i = 0; i < params.length; i++) {
                    var key = params[i].split('=')[0];
                    var value = decodeURIComponent(params[i].split('=')[1]);
                    queryString[key] = value;
                }
            }
              if (queryString["ContRent"] != null)
              {
                $scope.HdRentID = queryString["ContRent"].toString();
                $scope.BindRent(queryString["ContRent"].toString());
                $scope.BindRentTariffRecords(queryString["ContRent"].toString());
            }
         });   
        $scope.BindRent = function (ContRentID) {

                var Datavalue = {};
                var id = ContRentID;
                if (id == "" || id == null)
                Datavalue.ID = 0;
            else
                 Datavalue.ID = ContRentID;
          

                $http({
                    method: 'POST',
                    url: '/api/ContainerApi/ContainerRentParticular/',
                    data: JSON.stringify(Datavalue),
                }).then(function (result) {
                    $timeout(function () {

                        if (Datavalue.ID != "") {
                            $scope.ddlTariffType = { ID: result.data[0].TariffTypeID };
                            $scope.ddlSizeType = { TypeID: result.data[0].ContainerType };
                            $scope.ddlChargeType = { ID: result.data[0].ChargeTypeID };
                            $scope.ddlCharges = { ID: result.data[0].ChargesID };
                            $scope.ddlChargeOwner = { ID: result.data[0].ChargeOwnerID };
                            $scope.ddlShipmentType = { ID: result.data[0].ShipmentTypeID };
                            $scope.ddlPort = { ID: result.data[0].PortID };
                            $scope.ddlCustomer = { ID: result.data[0].CustomerID };
                            $scope.ddlTerminal = { ID: result.data[0].TerminalID };
                            $scope.ddlDepot = { ID: result.data[0].DepotID };
                            $scope.ddlStartingMove = { ID: result.data[0].StartingMove };
                            $scope.ddlEndingMove = { ID: result.data[0].EndingMove };
                            $scope.ddlAutoRunType = { ID: result.data[0].AutoRunTypeID };
                            $scope.txtDtValidFrom = result.data[0].ValidFrom;
                            $scope.txtDtValidTill = result.data[0].ValidTill;
                            $scope.ChkStatus = result.data[0].Status;

                        }
                        else {
                          
                        }
                     
                    });
                    }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
                });
               

            }

         $scope.BindRentTariffRecords = function (ContRentID) {

            var BussData = {}
         
            BussData.RentID = ContRentID;
          

            $http({
                method: 'POST',
                url: "/api/ContainerApi/ContainerRentTariffDtls",
                data: JSON.stringify(BussData),
            }).then(function (result)
            {
               
            $scope.DynamicGrid = [{ CID: "", SlabFrom: "", SlabTo: "", CurrencyID: "", Currency: "", Amount: "" }];
                
                $timeout(function () {

                    if (result.data[0].CurrencyID != "") {
                        $scope.DynamicGrid.splice(0, 1);
                    }

                    angular.forEach(result.data, function (value, key) {
                        
                        $scope.DynamicGrid.push({
                            'CID': result.data[key].CID,
                             'SlabFrom': result.data[key].SlabFrom,
                            'SlabTo': result.data[key].SlabTo,
                            'CurrencyID': result.data[key].CurrencyID,
                            'Currency': result.data[key].Currency,
                            'Amount': result.data[key].Amount,
                        });

                    });
                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });

        }
    });

    function ShowPopup(message) {
        swal(message);
    };

</script>

<div ng-app="MyApp" ng-controller="MyController">
    <div class="container-fluid">
        <div class="row">
