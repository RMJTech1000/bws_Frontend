<script type="text/javascript">



    LayoutApp.controller('MyController', function ($scope, $http, $timeout) {
        //$('#overlay').show();
        $(document).ready(function () {
            $('.my-select').select2({
                placeholder: "--selectvvv--",
                allowClear: true,
                width: '100%'
            });
        });
        $scope.ChargeCodeAllvalues = [
            { 'ChargeCode': 'PORT STORAGECHARGES', 'ID': 42 }
        ];

        $scope.ShipmentTypesAllvalues = [
            { 'ShipmentType': 'Export', 'ID': 1 },
            { 'ShipmentType': 'Import', 'ID': 2 }

        ];


        $scope.BLNumberlist = null;
        $scope.fillBlNumber = function ()
        {

          
            var Datavalue = {};
            Datavalue.AgencyID = localStorage.getItem("AgencyID");
            $http({
                method: 'POST',
                url: '/api/OnBard_shipment/BLNumbervalues/',
                data: JSON.stringify(Datavalue),
            }).then(function (result) {
                $timeout(function () {
                    $scope.BLNumberlist = result.data;
                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected. Please reconnect and try again.");
                }
            });

        };
        $scope.fillBlNumber();
        

        $(document).ready(function () {

            var queryString = new Array();
            if (window.location.search.split('?').length > 1) {
                var params = window.location.search.split('?')[1].split('&');
                for (var i = 0; i < params.length; i++) {
                    var key = params[i].split('=')[0];
                    var value = decodeURIComponent(params[i].split('=')[1]);
                    queryString[key] = value;
                }
            }
            if (queryString["BLID"] != null) {

                $scope.RegId = queryString["BLID"].toString();
                $scope.FilImp_PortStorage_CntrDetDetails(queryString["BLID"].toString());
                $scope.fillExistingEstimantDetails(queryString["BLID"].toString());
                //$scope.FilImpBookingCntrDetDetails(queryString["BkgID"].toString(), queryString["ImpReg"].toString());
                //$scope.OnclickRetriveDetails();
               
            }
        });




        $("#ddlBLNumber").change(function () {

            $scope.FilImp_PortStorage_CntrDetDetails($("#ddlBLNumber").val());
            $scope.fillExistingEstimantDetails($("#ddlBLNumber").val());
        });
        $scope.Imp_Port_Sotrage_CntrDetDetailsTypes = null;
        $scope.FilImp_PortStorage_CntrDetDetails = function (BLIDv)
        {
            $('#overlay').show();
            var Datavalue = {};
            Datavalue.BLID = BLIDv;
            Datavalue.ShipmentTypeID = "2";
            Datavalue.AgencyID = localStorage.getItem("AgencyID");

            $http({
                method: 'POST',
                url: '/api/PortStorage/Imp_PortStorageContainerWise',
                data: JSON.stringify(Datavalue),

            }).then(function (result) {
                $timeout(function ()
                {
;
                    if (result.data.length > 0)
                    {
                        $scope.ImpBindBLNumber = result.data[0].BLNo;
                        $scope.ContractID = result.data[0].ID;
                        //one1
                        $scope.Imp_Port_Sotrage_CntrDetDetailsTypes = result.data;
                    }

                    $('#overlay').hide();

                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }

        $scope.GridvalueDetention = null;


        $scope.ExpExistingEstimantDetails = null;
        $scope.fillExistingEstimantDetails = function (BLIDv) {

            var Datavalue = {};
            Datavalue.BLID = BLIDv;
            $http({
                method: 'POST',
                url: '/api/PortStorage/Port_EstimateexsitingValues',
                data: JSON.stringify(Datavalue),

            }).then(function (result) {
                $timeout(function () {
                    $scope.ExpExistingEstimantDetails = result.data;

                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });



        }




        $scope.SavedetReceord = function (gRow) {
            console.log(gRow);
            var CntrIDv = "";
            var Datavalue = {};
            var StrDate = "";
            angular.forEach(gRow, function (value, key)
            {
                if (typeof gRow[key]["IsTrue"] != "undefined") {
                    if (gRow[key]["IsTrue"] == "1") {
                        IDv = gRow[key]["ID"].toString();
                        CntrIDv += gRow[key]["CntrID"].toString() + ",";
                    }
                }
            });

            angular.forEach(gRow, function (value, key) {
                if (typeof gRow[key]["IsTrue"] != "undefined") {
                    if (gRow[key]["IsTrue"] == "1") {
                        StrDate += gRow[key]["StartDate"].toString();
                    }
                }
            });


            var validation = "";
            var ddlCharges = $('#ddlCharges').val();
            if (ddlCharges == "?") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Charge Code</span></br>"
            }
            if (CntrIDv == "") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Container No</span></br>"
            }

            if (StrDate == "") {

                validation += "<span style='color:red;'>*</span> <span>Check Start Date</span></br>"
            }


            if (validation != "") {
                ShowPopup(validation);
                return false;
            }
            Datavalue.BLID = $('#ddlBLNumber').val()
            Datavalue.CntrIDs = CntrIDv;
            Datavalue.TillDate = $('#txtCalTill').val();
            Datavalue.ChargeID = $('#ddlCharges').val();
            Datavalue.ShipmentTypeID = "2";
            Datavalue.AgencyID = localStorage.getItem("AgencyID");
            Datavalue.ChargeValue = $("#ddlCharges option:selected").text() + " CALCULATED SUCCESSFULLY";

            $('#overlay').show();

            $http({
                method: 'POST',
                url: '/api/PortStorage/Import_PortStorageInsert',
               // url: '/api/ImportApi/ImpDDGInsert',
                data: JSON.stringify(Datavalue),

            }).then(function (result) {
                $timeout(function () {
                    if (result.data.length > 0) {

                        $('#overlay').hide();
                      //  $scope.fillExistingEstimantDetails();
                        ShowPopup(result.data[0]["StatusAlery"]);


                    }
                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });


        }

        $scope.OpenEstimateValue = function (Idv,ChargeID)
        {

            var SumTotalPerCntr = 0;
            var SunTotalAmount = 0;
            var Datavalue = {};
            Datavalue.ID = Idv;
            $http({
                method: 'POST',
                url: '/api/PortStorage/DisplayPort_EstimateexsitingValues',
                data: JSON.stringify(Datavalue),

            }).then(function (result) {
                $timeout(function () {
                    $('#demoModalEstimate').modal('show');
                   // $scope.GridvalueDetention.length = 0;
                    $scope.GridvalueDetention = result.data[0].ItemListDDG;
                   
                    $scope.OnBilledAmountCheck(Idv, ChargeID);

                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }


        $scope.DeleteEstimate = function (gRow) {

            var IDv = "";
            var Datavalue = {};
            angular.forEach(gRow, function (value, key) {
                if (typeof gRow[key]["IsTrue"] != "undefined") {
                    if (gRow[key]["IsTrue"] == "1") {
                        IDv = gRow[key]["ID"].toString();
                    }
                }
            });
            Datavalue.ID = IDv;
            $http({
                method: 'POST',
                url: '/api/PortStorage/Port_DDGDelete',
                data: JSON.stringify(Datavalue),

            }).then(function (result) {
                $timeout(function () {
                    if (result.data.length > 0) {
                        $('#overlay').hide();

                        $scope.fillExistingEstimantDetails($scope.RegId);
                        if (result.data[0].StatusAlery == "Deleted")
                            ShowPopup("Estimate Delete sucessfully ");
                        else
                            ShowPopup("Invoice Exist Please contact administrator");

                    }
                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }


        $scope.InserDtvalueInvoice = function () {
            var validation = "";
            var DDIDv = 0;
            angular.forEach($scope.ExpExistingEstimantDetails, function (value, key) {
                if (typeof $scope.ExpExistingEstimantDetails[key]["IsTrue"] != "undefined") {
                    if ($scope.ExpExistingEstimantDetails[key]["IsTrue"] == "1") {
                        DDIDv = $scope.ExpExistingEstimantDetails[key]["ID"].toString();

                    }
                }
            });
            if (DDIDv == "") {
                validation += "<span style='color:red;'>*</span> <span>Please Select Container No</span></br>"
            }
            if (validation != "") {
                ShowPopup(validation);
                return false;
            }
            var Datavalue = {};
            Datavalue.BkgID = $scope.RegId;
            Datavalue.BLID = $scope.HDBLId;
            Datavalue.DDID = DDIDv;
            Datavalue.PaymentModeID = 19;

            $http({
                method: 'POST',
                url: '/api/ImportApi/ImpDDGvalueInsertInvoiceTable',
                data: JSON.stringify(Datavalue),

            }).then(function (result) {
                $timeout(function () {
                    if (result.data.length > 0) {
                        $('#overlay').hide();

                        ShowPopup("Detintion invoice as Create Successfully");

                    }
                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });


        }

        $scope.OnBilledAmountCheck = function (DDID, ChargeIDv) {

            var Datavalue = {};
            Datavalue.DDID = DDID;
            Datavalue.ChargeID = ChargeIDv

            $http({
                method: 'POST',
                url: '/api/PortStorage/Port_DDGBilledAmount',
                data: JSON.stringify(Datavalue),

            }).then(function (result) {
                $timeout(function () {
                    $scope.TotalAmount = result.data[0]["TotalAmount"].toString();
                    $scope.TotalBilledAmount = result.data[0]["BilledAmount"].toString();
                    $scope.ExchangRate = result.data[0]["ExRate"].toString();
                    $scope.FreeDaysRate = result.data[0]["WaiverAmt"].toString();
                    $scope.TotalBilledAmountvalues = ((($scope.TotalAmount) - ($scope.TotalBilledAmount)) - ($scope.FreeDaysRate));
                    $scope.Voyage = result.data[0]["BLVesVoy"].toString();
                    $scope.ContractID = result.data[0]["ContID"].toString();

                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });
        }

    });
    function ShowPopup(message) {
        swal(message);
    };
</script>
