                                                                            <td>
                                                                                {{i.CollectionModeV}}

                                                                            </td>
                                                                            <td>  {{i.OwnershipV}} </td>
                                                                            <td>
                                                                                {{i.ShipmentTypeV}}
                                                                            </td>
                                                                            <td class="td-actions text-right actwid">
                                                                                <button type="button" rel="tooltip" ng-click="PrincipalEdit(i.RID)" class="btn btn-primary btn-round" data-original-title="" title="">
                                                                                    <i class="material-icons">edit</i>
                                                                                </button>

                                                                            </td>
