                                //    $scope.DynamicGridCostLocalExport.push({

                                //        'ChargeCode': $scope.SearchTariffRateList[key].ChargeCode,
                                //        'Currency': $scope.SearchTariffRateList[key].Currency,
                                //        'CntrType': $scope.SearchTariffRateList[key].CntrType,
                                //        'Mode': $scope.SearchTariffRateList[key].Mode,
