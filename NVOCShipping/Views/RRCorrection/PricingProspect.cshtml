            Data.POLID = $("#ddlFromPort").val();
            Data.PODID = $("#ddlToPort").val();
            Data.CntrTypeId = $("#ddlCntrTypes").val();
            Data.CommodityID = $("#ddlCommodityType").val();
            $http({
                method: 'POST',
                url: '/api/RRCorrection/RRPricingView',
                data: JSON.stringify(Data),

            }).then(function (result) {
                $timeout(function ()
                {
                    if (result.data.length > 0)
                    {
                        
                        $scope.SearchTariffRateList = result.data;
                        $scope.DynamicGridFreight.length = 0;
                        $scope.DynamicGridLocalImport.length = 0;
                       // $scope.DynamicGridLocalExport.length = 0;
                        $scope.DynamicGridCostLocalImport.length = 0;
                        //$scope.DynamicGridCostLocalExport.length = 0;


                        angular.forEach($scope.SearchTariffRateList, function (value, key)
                        {
                            
                            if ($scope.SearchTariffRateList[key].TraiffRegular == "135")
                            {
                                $scope.DynamicGridFreight.push({

                                    'ChargeCode': $scope.SearchTariffRateList[key].ChargeCode,
                                    'Currency': $scope.SearchTariffRateList[key].Currency,
                                    'CntrType': $scope.SearchTariffRateList[key].CntrType,
                                    'Mode': $scope.SearchTariffRateList[key].Mode,
                                    'Amount': $scope.SearchTariffRateList[key].Amount
                                })
                                $scope.FreightUSDAmt += parseFloat($scope.SearchTariffRateList[key].Amount / parseFloat($scope.SearchTariffRateList[key].ExRate));
                                $scope.FreightExRate = parseFloat($scope.SearchTariffRateList[key].ExRate);

                            }

                            if ($scope.SearchTariffRateList[key].TraiffRegular == "138" || $scope.SearchTariffRateList[key].TraiffRegular == "136")
                            {
                                    if ($scope.SearchTariffRateList[key].ChargeTypeID == "1" && $scope.SearchTariffRateList[key].CollectionModeID == "18")
                                    {
                                        $scope.DynamicGridLocalImport.push({

                                            'ChargeCode': $scope.SearchTariffRateList[key].ChargeCode,
                                            'Currency': $scope.SearchTariffRateList[key].Currency,
                                            'CntrType': $scope.SearchTariffRateList[key].CntrType,
                                            'Mode': $scope.SearchTariffRateList[key].Mode,
                                            'Amount': $scope.SearchTariffRateList[key].Amount
                                        })

                                        $scope.POLChargeUSDAmt += parseFloat($scope.SearchTariffRateList[key].Amount / parseFloat($scope.SearchTariffRateList[key].ExRate));
                                        $scope.POLChargeExRate = parseFloat($scope.SearchTariffRateList[key].ExRate);
                                    }
                                    //if ($scope.SearchTariffRateList[key].ChargeTypeID == "1" && $scope.SearchTariffRateList[key].CollectionModeID == "19") {
                                    //    $scope.DynamicGridLocalExport.push({

                                    //        'ChargeCode': $scope.SearchTariffRateList[key].ChargeCode,
                                    //        'Currency': $scope.SearchTariffRateList[key].Currency,
                                    //        'CntrType': $scope.SearchTariffRateList[key].CntrType,
                                    //        'Mode': $scope.SearchTariffRateList[key].Mode,
                                    //        'Amount': $scope.SearchTariffRateList[key].Amount
                                    //    })
                                    //    $scope.PODChargeUSDAmt += parseFloat($scope.SearchTariffRateList[key].Amount);
                                    //    $scope.PODChargeExRate = parseFloat($scope.SearchTariffRateList[key].ExRate);
                                    //}
                            }


