                        {

                            

                            if ($scope.SearchTariffRateListPOD[key].TraiffRegular == "138" || $scope.SearchTariffRateListPOD[key].TraiffRegular == "136")
                            {
                                
                                if ($scope.SearchTariffRateListPOD[key].ChargeTypeID == "1" && $scope.SearchTariffRateListPOD[key].CollectionModeID == "19") {
                                    $scope.DynamicGridLocalExport.push({

                                        'ChargeCode': $scope.SearchTariffRateListPOD[key].ChargeCode,
                                        'Currency': $scope.SearchTariffRateListPOD[key].Currency,
                                        'CntrType': $scope.SearchTariffRateListPOD[key].CntrType,
                                        'Mode': $scope.SearchTariffRateListPOD[key].Mode,
                                        'Amount': $scope.SearchTariffRateListPOD[key].Amount
                                    })
                                    $scope.PODChargeUSDAmt += parseFloat($scope.SearchTariffRateListPOD[key].Amount / parseFloat($scope.SearchTariffRateListPOD[key].ExRate));
                                    $scope.PODChargeExRate = parseFloat($scope.SearchTariffRateListPOD[key].ExRate);
                                }
                            }


                            if ($scope.SearchTariffRateListPOD[key].TraiffRegular == "138" || $scope.SearchTariffRateListPOD[key].TraiffRegular == "136") {
                                if ($scope.SearchTariffRateListPOD[key].ChargeTypeID == "2" && $scope.SearchTariffRateListPOD[key].CollectionModeID == "19") {
                                    $scope.DynamicGridCostLocalExport.push({

                                        'ChargeCode': $scope.SearchTariffRateListPOD[key].ChargeCode,
                                        'Currency': $scope.SearchTariffRateListPOD[key].Currency,
                                        'CntrType': $scope.SearchTariffRateListPOD[key].CntrType,
                                        'Mode': $scope.SearchTariffRateListPOD[key].Mode,
                                        'Amount': $scope.SearchTariffRateListPOD[key].Amount
                                    })
                                    $scope.PODCostUSDAmt += parseFloat($scope.SearchTariffRateListPOD[key].Amount / parseFloat($scope.SearchTariffRateListPOD[key].ExRate));
                                    $scope.PODCostExRate = parseFloat($scope.SearchTariffRateListPOD[key].ExRate);
                                }
                            }

                        });

                      
                        $scope.SearchTariffSlotRate();
                        $("#overlay").hide();

                    }

                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });

        }



        $scope.SearchTariffSlotRate = function () {

          
            $("#overlay").show();
            var Data = {};
            Data.POLID = $("#ddlFromPort").val();
            Data.PODID = $("#ddlToPort").val();
            Data.CntrTypeId = $("#ddlCntrTypes").val();
            Data.CommodityID = $("#ddlCommodityType").val();
            $http({
                method: 'POST',
                url: '/api/RRCorrection/RRPricingSlotView',
                data: JSON.stringify(Data),

            }).then(function (result)
            {
                $timeout(function () {
                    if (result.data.length > 0)
                    {

                        $scope.DynamicGridChargeSlotAmount = result.data;
                        angular.forEach($scope.DynamicGridChargeSlotAmount, function (value, key) {

                            $scope.SlotChargeExRate += parseFloat($scope.DynamicGridChargeSlotAmount[key].ExRate);
                            $scope.SlotChargeUSDAmt = parseFloat($scope.DynamicGridChargeSlotAmount[key].SlotAmt20) + parseFloat($scope.DynamicGridChargeSlotAmount[key].SlotAmt40);
                        });
                       
                        $("#overlay").hide();

                    }

                });
            }, function (result) {

                if (result.status > 0) {
                    var errorMsg = result.status + ': ' + result.data;
                    ShowPopup("Sorry no internet connectivity detected.Please reconnect and try  again.");
                }
            });


            $scope.RevenueAmt = (parseFloat($scope.FreightUSDAmt) +
                parseFloat($scope.POLChargeUSDAmt) + parseFloat($scope.PODChargeUSDAmt)).toFixed(3);

            $scope.CostAmt = (parseFloat($scope.SlotChargeUSDAmt) +
                parseFloat($scope.POLCostUSDAmt) + parseFloat($scope.PODCostUSDAmt)).toFixed(3);
            $scope.ProfitAmt = (parseFloat($scope.RevenueAmt) - parseFloat($scope.CostAmt)).toFixed(3);

            $scope.BrakupPOL = $("#ddlFromPort option:selected").text();
            $scope.BrakupPOD = $("#ddlToPort option:selected").text();
        }




        $scope.RRonclick = function () {


            $('#demoModal').modal('show');


        }
